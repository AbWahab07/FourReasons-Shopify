        @extend %flexbox;
        @extend %align-items-center;
        span {
          @include margin-end(10px);
          @include flex-box(19px);
        }
      }
    }
  }
  .custom-block-html {
    @extend %font-size-1;
  }
  .custom-block-body-text-2 {    
    line-height: 1.6;
  }
  .button-group {        
    a {              
      &:not(.btn) {
        display: inline-block;
        position: relative;
        &:before {
          content: '';					
          height: 1px;	
          width: 0;		
          background: currentColor;
          @include ab(n, 0, -4px, n);
          @extend %transition-ease;
          @extend %opacity-zero;
        }
        &:after {
          @extend %before-expand;
          @extend %transition-ease;
          @extend %opacity-zero;
          @include ab(2px, n, n, -5px);
          font-size: 12px;
          color: inherit;
        }
        &:hover {
          &:before {
            @include opacity(1);
            width: 100%;
          }
          &:after {
            @include opacity(1);          
            @if $enable_rtl {
              left: -15px;
            }
            @else {
              right: -15px;
            }
          }
        }
      }      
    }         
  }
  &.text-center {
    ul, ol {
      li {
        @extend %justify-content-center;
      }
    }
  }
  &.text-right {
    ul, ol {
      li {
        @extend %justify-content-flex-end;
      }
    }
  }
