     }
    }
  }
  &.effect-fade_in, &.effect-fade_out {
    .card-header .card-image a span.image--style {
      @extend %transition-ease;
    }
  }
  /*Effect fade in*/
	&.effect-fade_in {
    @media (min-width: 1025px) {
      .card-header .card-image a span.image--style {
        @include opacity(0.3);
      }
    }
		&:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a span.image--style {
          @include opacity(1);      					
        }
      }
		}
  }
  /*Effect fade out*/
  &.effect-fade_out {	
		&:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a span.image--style {
          @include opacity(0.3);
        }
      }
		}
  }
  /*Back Img Fade In*/
  &.effect-fade_in_back {
    .card-header {
      .next-image,
      .prev-image {
        display: none;
      }
      &:hover {
        @media (min-width: 1025px) {
          .card-image a > span.img-back img {
            @include opacity(1);
            visibility: visible;
          }
        }
      }
    }    
  }
  /*Back Img Zoom In*/
	&.effect-zoom_in {
    .card-header {
      .next-image,
      .prev-image {
        display: none;
      }
      .card-image a > span.img-back img {
        @include scale(1.1, 1.1);
        @extend %translate-zero;
        left: 0;
        top: 0;
      }
      &:hover {
        @media (min-width: 1025px) {
          .card-image a {            
            > span.img-back img {
              @include opacity(1);
              @include scale(1.15, 1.15);
              visibility: visible;
            }        
          }
        }
      }
    }    
  }
  /*Effect zoom*/
  &.effect-zoom {
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img {
          @include scale(1.12, 1.12);
        }
      }
    }
  }
  /*Effect Zoom Fade*/
  &.effect-zoom_fade {
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img.featured-image {
          @include scale(1.1, 1.1);
          @include opacity(0.7);
        }
      }
    }
  }
  /*Effect Blur*/ 
  &.effect-blur {
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img {
          -webkit-filter: blur(5px);
          filter: blur(5px);
        }
      }
    }
  }
  /*Effect Glow*/
  &.effect-glow {
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img {
          -webkit-filter: brightness(1.1);
          filter: brightness(1.1);
        }
      }
    }
  }
  /*Effect Color*/
  &.effect-color {
    .card-header .card-image a img {
      -webkit-filter: grayscale(100%);
			filter: grayscale(100%);
    }
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img {
          -webkit-filter: grayscale(0);
          filter: grayscale(0);
        }
      }
    }
  }
  /*Effect Grayscale*/
  &.effect-grayscale {
    &:hover {
      @media (min-width: 1025px) {
        .card-header .card-image a img {
          -webkit-filter: grayscale(100%);
          filter: grayscale(100%);
        }
      }
    }
  }
  &:hover {
    @media (min-width: 1025px) {
      .label-soldout {
        visibility: hidden;
      }
      .card-header .card-image {       
        .prev-image,
        .next-image {
          @include translateX(0);
          @include opacity(1);
        }
      }
    }
  } 
}
