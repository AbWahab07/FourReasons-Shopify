.owl-nav {
  z-index: 999;   
  > button {
    @include ab(calc(50% - 25px), n, n, n);    
    @extend %transition-ease;   
    @extend %opacity;       
    visibility: hidden;
    @media (max-width: 1024px) {
      @include opacity(1);
      visibility: visible;
    }  
    span {            
      width: 50px;
      height: 50px;            
      text-align: center;
      outline: none; 
      line-height: 50px;     
      color: $nav-owl-color;    
      @if $nav-owl-bg != rgba(0,0,0,0) {
        background: rgba($nav-owl-bg, ($nav-owl-opacity / 100)); 
      }
      display: block;
      @media (max-width: 1024px) {
        width: 40px;
        height: 40px;
        line-height: 40px;
      }
      &:before {                        
        color: inherit;        
        @extend %font-small-icon;    
        @extend %font-family;             
      }      
    }
    &:hover {
      @include opacity(0.7);
      visibility: visible;
    }
  }
  .owl-prev {
    @media (min-width: 1025px) {
      left: -90px;
    }
    @media (max-width: 1024px) {
      left: -15px;
    }
    span {      
      &:before {
        content: '\ea89';
      }
    }
  }
  .owl-next {    
    @media (min-width: 1025px) {
      right: -90px;
    }
    @media (max-width: 1024px) {
      right: -15px;
    }
    span {      
      &:before {      
        content: '\ea8b';
      }
    }
  }
}
.owl-dots {
  @include translateX(-50%);
  @include ab(n, 50%, 0, n);
  @extend %absolute-content;  
  pointer-events: inherit;  
  .owl-dot {
    display: inline-block;
    margin: 10px 8px 0;
    opacity: 0.7;
    span {
      display: block;
      width: 12px;
      height: 12px;
      background: $main-color;
      @extend %border-radius-circle;
    }
    &.active {
      opacity: 1;
    }
  }
}
.templateIndex {
  #body-content .shopify-section {
    > div {
      overflow: hidden;    
      > div {
        position: relative;        
      }
      &.home-product-lookbook,
      &.home-pricing-plan, 
      &.home-banner,
      &.home-single-banner,
      &.section-overlay {
        overflow: visible;        
        > div {
          z-index: 2;
        }
      }
    }
    .owl-loaded {
      &:hover {    
        @media (min-width: 1025px) {       
          .owl-nav .owl-next {
            right: -15px;
            visibility: visible;
          }
          .owl-nav .owl-prev {
            left: -15px;
            visibility: visible;
          }
        }
        @media (min-width: 1600px) {
          .owl-nav .owl-next {
            right: -78px;
          }
          .owl-nav .owl-prev {
            left: -78px;
          }
        }
      }
    }
    &#shopify-section-blog-template {
      > div {
        overflow: visible;
      }
    }
  }
}
.layout-full {
  .owl-loaded {    
    &:hover {
      @media (min-width: 1025px) {
        .owl-nav .owl-next {
          right: 0 !important;        
        }
        .owl-nav .owl-prev {
          left: 0 !important;        
        }
      }
    }
    @media (max-width: 1024px) {
      .owl-nav .owl-next {
        right: 0;
      }
      .owl-nav .owl-prev {
        left: 0;
      }
    }
  }
}

