@mixin text-shadow-ps {
  text-shadow:  0 0 10px #3498DB,
                0 0 20px #3498DB,
                0 0 30px #3498DB,
                0 0 40px #2980B9,
                0 0 70px #2980B9,
                0 0 80px #2980B9,
                0 0 100px #2980B9,
                0 0 150px #2980B9;
}
@mixin prefix($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else if $prefix == moz {
      -moz-#{$property}: $value;
    } @else if $prefix == ms {
      -ms-#{$property}: $value;
    } @else if $prefix == o {
      -o-#{$property}: $value;
    } @else if $prefix == spec {
      #{$property}: $value;
    } @else  {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}
@mixin transform-origin($origin) {
  @include prefix('transform-origin', $origin, moz ms webkit spec);
}
@mixin box-shadow($shadow...) {
  @include prefix('box-shadow', #{$shadow}, moz ms webkit spec);
}
@mixin transition($transition...) {
  @include prefix(transition, $transition, ms webkit spec);
}
@mixin transform($transform) {
  @include prefix(transform, $transform, ms webkit spec);
}
@mixin flex($value: 0 1 auto) {
  @include prefix(flex, $value, webkit moz ms spec);
}
@mixin flex-wrap($wrap) {
  @include prefix(flex-wrap, $wrap, webkit moz ms spec);
}
@mixin flex-direction($direction) {
  @include prefix(flex-direction, $direction, webkit moz ms o spec);  
}
@mixin flex-basis($width: auto) { 
  -ms-flex-preferred-size: $width;
  @include prefix(flex-basis, $width, webkit moz spec);
}
@mixin align-self($align: auto) { 
  -ms-flex-item-align: $align;
  @include prefix(align-self, $align, webkit spec);
}
@mixin flex-grow($grow) {
  @include prefix(flex-grow, $grow, webkit spec);
}
@mixin align-content($align: center) {
  @include prefix(align-content, $align, webkit ms spec);
}
@mixin justify-content($justify: flex-start) {
  @include prefix(justify-content, $justify, webkit ms spec);
}
@mixin align-items($value: stretch) {
  $alt-value: $value;
  @if $value == 'flex-start' {
    $alt-value: start;
  } @else if $value == 'flex-end' {
    $alt-value: end;
  }  
  -ms-flex-align: $alt-value;
  @include prefix(align-items, $value, webkit moz ms o spec);
}
@mixin translateX($x) {
  @if $enable_rtl {
    @include transform(translateX($x * (-1)));
  }
  @else {
    @include transform(translateX($x));
  }
}
@mixin translateY($y) {
  @include transform(translateY($y));
}
@mixin translate($x, $y) {  
  @if $enable_rtl {    
    @include transform(translate($x *(-1), $y));
  }
  @else {    
    @include transform(translate($x, $y));
  }
}
@mixin translate3d($x, $y, $z) {
  @include transform(translate3d($x, $y, $z));
}
@mixin skew($x, $y) {
  @include transform(skew(#{$x}deg, #{$y}deg));
}
@mixin scale($x, $y){
  @include transform(scale($x, $y));  
}
