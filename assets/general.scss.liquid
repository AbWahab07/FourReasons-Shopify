    height: 100%;
    max-height: calc(100vh - 40px); 
    overflow-y: auto;
    @extend %flexbox;
    @extend %flex-wrap;
    .block-image {        
      position: relative;
      span {
        height: 100%;          
      }
      img {
        @include ab(0, 0, n, n);
        height: 100%;
        width: 100%;
        object-fit: cover;
        font-family: "object-fit: cover";
      }
    }
  }
  .newsletter-popup-content {
    display: block;      
    width: 100%;
    height: 100%;
    max-height: 95vh;   
    position: relative; 
    max-height: calc(100vh - 40px);           
    @extend %border-radius-5;
    .np-close {
      @extend %close-btn;
    }
  }
  .block-image-true {
    .text-box {
      margin: 25px;
      @include box-shadow(0 0 6px rgba($dark-color, 0.5)); 
      @media (max-width: 767px) {
        margin: 15px;
      }    
      &.position-right {
        @media (min-width: 768px) {
          @include flex-box(50%);
          @include flexbox;
          @include justify-content(center);
          @include flex-direction(column);
          @include border-radius(5px);            
          left: calc(50% - 30px);   
          margin-left: 0;
          margin-right: 0;  
        }
        @media (max-width: 767px) {
          float: none;
          width: calc(100% - 30px);
          @include margin-start(15px);
        }        
      }
      &.position-left {                    
        @media (min-width: 768px) {
          @include flex(0 0 calc(50% + 5px));
          @include flexbox;
          @include flex-direction(column);
          @include justify-content(center);
          @include border-radius(5px);
          max-width: calc(50% + 5px);   
          @include margin-end(-30px);         
        }
        @media (max-width: 767px) {            
          width: calc(100% - 30px);
          @include margin-end(15px);
        }          
      }
      &.position-top {        
        @media (min-width: 768px) {
          @include flex(0 0 calc(100% - 50px));
          max-width: calc(100% - 50px);
        }
        @media (max-width: 767px) {
          margin-bottom: 15px;
          @include flex(0 0 calc(100% - 30px));
          max-width: calc(100% - 30px);
        }
      }
      &.position-bottom {        
        @media (min-width: 768px) {
          @include flex(0 0 calc(100% - 50px));
          max-width: calc(100% - 50px);
        }
        @media (max-width: 767px) {
          margin-bottom: 15px;
          @include flex(0 0 calc(100% - 30px));
          max-width: calc(100% - 30px);
        }
      }
      &.position-middle {   
        margin-bottom: -43%;       
        @media (min-width: 768px) {
          // @include flexbox;
          // @include flex-direction(column);
          // @include justify-content(center);
          // @include flex(0 0 calc(100% - 50px));
          // max-width: calc(100% - 50px);          
        }
        @media (max-width: 767px) {
          margin-bottom: 15px;
          @include flex(0 0 calc(100% - 30px));
          max-width: calc(100% - 30px);
        }
      }
      &.position-right,
      &.position-left,
      &.position-top,
      &.position-bottom,
      &.position-middle {
        .button-position-right {
          @media (max-width: 479px) {
            form {
              @include flex-wrap(wrap);
            }
            button {
              margin-top: 20px;
            }
          }
        }
      }
    }
    .block-image {
      &.image-position-right {
        @media (min-width: 768px) {
          @include flex-box(50%);
          right: 50%;
        }
        @media (max-width: 767px) {
          display: none;
        }
      } 
      &.image-position-left {
        @media (min-width: 768px) {
          @include flex-box(50%);          
        }
      }
      &.image-position-top,
      &.image-position-bottom,
      &.image-position-middle {
        max-height: 440px;  
        height: 100%;
        overflow: hidden;  
        @extend %flex-100;        
      }          
    }   
  }
  .box-countdown {
    overflow: hidden;
    @extend %border-radius-5;
  }
  .text-box {
    display: block;
    position: relative;
    padding: 35px 30px 30px;   
    z-index: 999;  
    background: $body-background-color;
    @extend %background-option;
    @media (max-width: 767px) {
      padding: 40px 15px 15px;
    }
    .text-box-content {
      margin-bottom: 23px;  
      @media (max-width: 767px) {
