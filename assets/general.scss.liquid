@mixin itemRange($pe){
  -webkit-box-flex: 0;
  -ms-flex: 0 0 #{$pe};
  flex: 0 0 #{$pe};
  max-width: #{$pe};
}
@mixin colWidth($i){
  $percent: 100/$i+'%';
  .col-#{$i}th{@include itemRange($percent)}
  @media (min-width: 576px) {
    .col-sm-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 768px) {
    .col-md-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 992px) {
    .col-lg-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 1200px) {
    .col-xl-#{$i}th{@include itemRange($percent)}
  }
}
@for $i from 7 to 13{@include colWidth($i)}

$light-color: {{settings.light_color}};
$dark-color: {{settings.dark_color}};
$body-font-size: {{settings.body_font_size}};
/*calculate YIQ and return text color follow Background*/
@function color-contrast($color, $dark: $dark-color, $light: $light-color) {
  @if $color == null {
    @return null;
  }
  @else {
    $yiq: (red($color) * 299 + green($color) * 587 + blue($color) * 114) / 1000;
    @return if($yiq >= 128, $dark, $light);
  }
}
@function pxtorem($target, $context: $body-font-size) {
  @return ($target / $context) * 1rem;
}


