.post-item {
	padding-bottom: 40px;		
	.post-header {		
		.post-header-inner {
			position: relative;
		}
		.post-label {	
			> span {
				position: absolute;
				z-index: 1;
			}		
			.post-tag {
				left: 0;
				top: 30px;
				padding: 4px 20px;
				line-height: 22px;
				display: inline-block;
				background: $main-color;
				color: $main-color-text;
				@extend %font-size-1;
			}
			.post-created {
				text-align: center;
				> span {
					display: block;
					line-height: 100%;
				}
				&.style-1,
				&.style-2 {										
					@extend %transition-ease;				
					span {
						color: inherit;
					}
					.day {
						font-size: $size-h2;
						padding: 10px 0 4px;
					}
					.mon {
						@extend %font-size-1;
					}
				}
				&.style-1 {	
					top: -30px;				
					width: 60px;
					height: 60px;	
					background: $body-background-color;					
					border: 1px solid $main-color;					
					@extend %main-color;
					.day {
						padding-top: 2px;
					}
				}
				&.style-2 {
					top: -36px;
					width: 76px;
					height: 76px;	
					background: $main-color;
					color: $main-color-text;					
				}
				&.style-3 {
					top: 12px;
					width: 60px;
					height: 60px;
					border: 4px solid $main-color;					
					background: $body-background-color;	
					@include border-radius(23px);						
					span {
						@extend %body-title-color;
						font-family: $font-heading;	
					}
					.day {						
						font-size: $size-h3;											
						padding: 6px 0 0;
						line-height: 1;
					}
					.mon {
						text-transform: uppercase;
					}
				}
				&.style-4 {
					top: -35px;
					padding: 26px 0;
					background-repeat: no-repeat;
					background-position: center;
					> span {
						display: block;
						color: $light-color;
						font-family: $font-heading;
						line-height: 1;						
						&.day {
							font-size: 23px;							
						}
						&.mon {
							font-size: 14px;
							margin-top: -2px;
						}
					}
				}
			}			
			&.pos-left {
				.post-created.style-1,
				.post-created.style-2,
				.post-created.style-4 {					
					@include ab(n, 0, n, n);
				}
				.post-created.style-3 {															
					@include ab(n, 15px, n, n);	
				}												
			}
			&.pos-right {
				.post-created.style-3 {										
					@include ab(n, n, n, 15px);	
				}
				.post-created.style-1,
				.post-created.style-2,
				.post-created.style-4 {					
					@include ab(n, n, n, 0);	
				}
				.post-tag {
					left: auto; 
					right: 0;
				}
			}		
			&.pos-center {
				.post-created {
					&.style-1, &.style-2, &.style-3, &.style-4 {
						@include translateX(-50%);						
						@include ab(n, 50%, n, n);					
					}			
				}	
			}
		}
	}
	.post-image {
		position: relative;
		overflow: hidden;		
		a {
			display: block;
			&:before {
				content: '';			
				z-index: 1;	
				@include ab(0, 0, 0, 0);
				@extend %transition-ease;
			}
			img {
				@include transition(all 0.4s ease-out);
				backface-visibility: hidden;
			}			
		}
		&.image-rd {
			@extend %border-radius-20;
			img {
				@extend %border-radius-20;
			}
		}		
	}		
	.post-body {
		overflow: hidden;
	}
	.post-body, .post-content { 		         
		.post-title {               					
			line-height: 1.2;			
			&.hl_primary {
				a {
					color: inherit;
				}
			}
		}
		.post-info {			
			@extend %body-light-color;			       
			> span {                  
				color: inherit; 
				&:not(:last-child) {
					@include padding-end(5px);
				}
				.icon-type {
					@include padding-end(7px);
				}
			}
		}
		.post-excerpt {
			@extend %body-light-color;			
			@include line-clamp(4);
			line-height: 24px;
			max-height: 96px;					 
		}
		.post-button {			
			.btn {
			 white-space: nowrap;
			 overflow: hidden;
			 text-overflow: ellipsis;
			 max-width: 100%;
			}			
		}
		.has-border {
			margin-top: 25px;
			padding: 50px 10px 40px 10px;
			@include border-radius(35px);
		}
		.blog-link:not(.btn), .btn-article:not(.btn) {
			display: inline-block;
			position: relative;
			line-height: 25px;
			color: $main-color;			
			&:before {
				content: '';					
				height: 1px;	
				width: 0;		
				background: $main-color;
				@include ab(n, 0, 0, n);
				@extend %transition-ease;
				@extend %opacity-zero;
			}
			&:after {
				@extend %before-expand;
				@extend %transition-ease;
				@extend %opacity-zero;
				@include ab(2px, n, n, -5px);
				font-size: 12px;
				color: inherit;
			}
			&:hover {
				&:before {
					@include opacity(1);
					width: 100%;
				}
				&:after {
					@include opacity(1);
					@if $enable_rtl {
						left: -15px;
					}
					@else {
						right: -15px;
					}
				}
			}
		}
	}	
	&.border-type-all {	      
		.post-body {
			.post-body-inner {
				padding: 0 50px 20px 50px;
				@media (max-width: 1199px) {					
					padding-left: 10%;
					padding-right: 10%;
				}
			}
		}
	}
	&.border-type-top {           
		.post-item-inner {
			border-width: 2px !important;
			padding-top: 40px;
			&.no-post-image {
				.post-body {
					padding: 0;
					border: none;
				}
			}
		}         
	}
	&.border-type-layer {           
		.post-header {
			padding-top: 10px;
			@include padding-end(10px);
			.post-image {
			 @include box-shadow(10px -10px 0 0 $main-color);                                
			}               
		}   
	}
	.post-item-inner {
		position: relative;		
		&.content-overlay {		
			margin-bottom: -10px;	
			.post-body {
				position: absolute;
				top: 0;
				left: 0;
				height: 100%;
				width: 100%;	
				pointer-events: none;
				z-index: 2;
				background: rgba($dark-color, 0.5);				
				@extend %flexbox;		
				@include transition(all 0.4s ease-out);	
				@media (min-width: 1025px) {
					@include opacity(0);
				}					 
				.post-body-inner {
					padding: 15px;
					width: 100%;
					color: $light-color;
					@extend %flexbox;
					@extend %justify-content-center;
					@extend %flex-direction-column;
					.post-title, .post-title a, .post-info, .post-excerpt {
						color: inherit;
						@include transition(none);
					}					
				}			
			}
		}
		&.no-post-image {
			.post-body {
				border: 2px solid $body-border-color;
				padding: 50px;
				@media (max-width: 1199px) and (min-width: 768px) {
					padding: 10%;
				}
				@media (max-width: 479px) {
					padding: 7%;
				}
				.post-body-inner {
					padding: 0;
				}
			}
		}
		&:hover {
			@media (min-width: 1025px) {
				.post-header .post-label .post-created.style-1 {
					background: $main-color;
					color: $main-color-text;
				}
				.post-header .post-label .post-created.style-2 {
					background: $body-title-color;
					color: $main-color-text;
				}
				&.content-overlay .post-body {
					@include opacity(1);
				}
			}
		}
	}	
}
.hover-style-zoom .post-item .post-header .post-image {
	&:hover {
		a {
			@media (min-width: 1025px) {
				&:before {
					background: rgba($dark-color, 0.2);
				}
				img {
					@include opacity(0.8);
					@include scale(1.1, 1.1);
				}
			}
		}
	}
}
.hover-style-overlay .post-item .post-header .post-image {
	&:hover {
		a {
			@media (min-width: 1025px) {
				&:before {					
					background: rgba( $main-color , 0.4);
				}				
			}
		}
	}
}
.has-shadow {
	.post-item .post-item-inner {	
		.post-header {
			margin-bottom: 25px;
		}
		.post-body {		
			@include box-shadow(0 3px 17px 4px $body-border-color);						
		}
	}
	&.owl-carousel {		
		margin-left: -15px;
		margin-right: -15px;
		width: auto;	
		&.owl-loaded {
			&:hover {
				@media (max-width: 1200px) {
				  .owl-nav .owl-next {
					right: 0;        
				  }
				  .owl-nav .owl-prev {
					left: 0;        
				  }
				}
			}
			@media (max-width: 1024px) {
				.owl-nav .owl-next {
					right: 0;
				}
				.owl-nav .owl-prev {
					left: 0;
				}
			}
		}		
		.post-item .post-item-inner {			
			padding-left: 15px;
			padding-right: 15px;
			padding-bottom: 20px;
		}
	}
}

#preview_demos {
    @include fixed(0, n, n, 0);
    z-index: 9;
    @media (max-width: 1199px) {
        display: none;
    }
    a {
        @include ab(190px, -50px, n, n);
        width: 50px;
        height: 60px;
        cursor: pointer;
        background: #222;  
        .preview_counting {
            display: block;
            width: 100%;
            text-align: center;
            position: relative;
        }          
        .preview_count {
            color: #fff;
            font-size: 32px;
            display: block;
            width: 100%;
            line-height: 56px;            
        } 
        .preview_icon {
            @include ab(0, 0, 0, 0);            
            @extend %transition-ease;
            @extend %opacity-zero;
            line-height: 1;       
            color: #fff;
            font-size: 22px;     
            line-height: 56px;
            text-align: center;
        } 
        &:hover {
            .preview_count {
                @extend %opacity-zero;
            }
            .preview_icon {
                @extend %opacity;
            }
        }  
    }
    .preview_text {
        @include ab(12px, 50%, n, n);
        @include transform(translate(-100%, 50%) rotate(90deg));
        background: #fff;
        color: #222;
        border: 1px solid #e1e1e1;
        padding: 3px 4px;
        margin: 0 0 0 -3px;
        font-size: 10px;
        line-height: 1;
        width: 60px;
        text-align: center;
        text-transform: uppercase;
    }
}
@-webkit-keyframes hvr-icon-float-away {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0;
        -webkit-transform: scale(1.8);
        transform: scale(1.8)
    }
}
@keyframes hvr-icon-float-away {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0;
        -webkit-transform: scale(1.8);
        transform: scale(1.8)
    }
}
#purchase_popup {
    position: fixed;
    right: 83px;
    bottom: 15px;
    z-index: 999;
    @media (max-width: 767px) {
        display: none;
    }
    > a {
        display: block;
        width: 85px;
        height: 89px;
        background: url({{ 'sprites.png' | asset_url }});
        background-repeat: no-repeat;
        background-position: -341px 1px;
        &:after {
            content: '';
            width: 60px;
            height: 60px;
            z-index: -1;
            -webkit-animation-duration: 1s;
            animation-duration: 1s;
            -webkit-animation-fill-mode: forwards;
            animation-fill-mode: forwards;
            -webkit-animation-name: hvr-icon-float-away;
            animation-name: hvr-icon-float-away;
            -webkit-animation-timing-function: ease-out;
            animation-timing-function: ease-out;
            -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite;
            background: #87c78a;
            @extend %border-radius-circle;
            @include ab(15px, n, n, 0);
        }
    }
    .purchase_popup-content {
        position: fixed;
        bottom: 100px;
        right: 70px;
        width: 430px;
        visibility: hidden;
        z-index: -1;
        background: #fff;
        @include box-shadow(0 0 11px $body-border-color);
        @include transition(all 0.3s ease-in-out 0s);
        .purchase_popup-row {
            padding: 30px;
            position: relative;
            p {
                font-size: 18px;
                color: #000;
                font-size: 700;
                margin: 0 0 10px;
            }            
            span {
                display: block;
                margin: 0 0 15px;
                line-height: 2em;                
            }
            > a {
                display: inline-block;
                padding: 7px 16px;
                line-height: 1.6;
                text-transform: uppercase;
                font-size: 12px;
                color: #fff;
                background: #7da72d;
                @include transition(.3s all ease-in-out);
                &:hover {
                    background: #577d0e;
                }
            }
            &:nth-child(1) {                
                z-index: 3;
                @include box-shadow(0 2px 4px $body-border-color);                   
            }
            &:nth-child(2) {
                background: #f7f7f7;                
                z-index: 2;
                > a {
                  background: #b9b9b9;  
                  &:hover {
                      background: #000;
                  }
                }
            }
        }
    }
    &:hover {
        .purchase_popup-content {
            visibility: visible;
            z-index: 999;
            
        }
    }
}