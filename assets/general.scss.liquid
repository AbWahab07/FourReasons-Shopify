              right: -15px;
            }
            @else {
              right: 15px;
            }
            @include translateX(5px);
            @media (min-width: 1025px) {
              &:hover {
                @include translateX(8px);
              }
            }
          }
          &:hover {
            .slick-arrow {
              @include opacity(1);
            }
          }
        }
