        margin-bottom: 0; 
      }
      .text {
        line-height: 1.2;
      }
    }
    .bold-text-true {
      font-weight: 700; 
    }
    input {
      width: 100%;
    }    
    .button-position-bottom {
      input {
        width: 100%;
        display: block;
        margin-bottom: 20px;
      }
      button {
        width: 100%;
        .icon-type + .btn-text {
          @include padding-start(8px);
        }
      }
    }
    .button-position-right {
      form { 
        width: 100%;       
        position: relative;        
        text-align: center;   
        @include flex-wrap(nowrap);       
        .btn {
          margin: 0;
        }        
      }
    }
    .block-social {
      text-align: center;
    } 
    .widget-social {
      margin: 0; 
      &.border-top-true {
        border-top: 1px solid $body-border-color;
        padding-top: 20px;          
      }
      li:not(:last-child) {
        @include padding-end(5px);
      }
      i {
        font-size: $font-icon;
      }
    }
    .countdown-content {        
      text-align: center;
      padding: 15px;        
      .offer-text {
        margin-bottom: 10px; 
        color: inherit;
        font-size: 120%;     
      }
      .wrapper-countdown > div {
        @extend %flexbox;
        @extend %align-items-center;    
        @extend %justify-content-center;
      }
      .wrapper-countdown > div > span {
        padding: 0 15px;          
        font-size: 140%;          
        @extend %flexbox;
        @extend %flex-direction-column;
        @extend %justify-content-center;
        @media (max-width: 479px) {          
          padding-left: 7px;
          padding-right: 7px;
        }
        .date-number {          
          color: inherit; 
          font-size: inherit;
        }
        .text-date {
          color: inherit; 
          @extend %font-size-4;
        }
      }      
    } 
  }
  .subscribe-result {
    .result-inner {
      padding: 45px 40px 40px;
      text-align: center;
      button {
        padding: 8px 50px;
        border: 1px solid currentColor;
      }
    }  
    .text-1 {
      margin: 0 0 30px;
      font-size: $size-h2;      
    } 
    .coupon-code {
      margin: 0 0 30px;
      .text-2 {
        margin-bottom: 15px; 
      }         
    } 
  }
}

