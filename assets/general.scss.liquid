@mixin ab($t, $l, $b, $r) {
  position: absolute;
  display: block;
  @if $enable_rtl {
    @if $l != n { right: $l; }
    @if $t != n { top: $t; }
    @if $r != n { left: $r; }
    @if $b != n { bottom: $b; }
  }
  @else{
    @if $l != n { left: $l; }
    @if $t != n { top: $t; }
    @if $r != n { right: $r; }
    @if $b != n { bottom: $b; }
  }
}
@mixin rotate($deg) {
  @include transform(rotate(#{$deg}deg));
}
@mixin rotateX($deg) {
  @include transform(rotateX(#{$deg}deg));
}
@mixin rotateY($deg) {
  @include transform(rotateY(#{$deg}deg));
}
@mixin placeholder {
  &::-webkit-input-placeholder {@content}
  &:-moz-placeholder           {@content}
  &::-moz-placeholder          {@content}
  &:-ms-input-placeholder      {@content}
}
@mixin before {
  content: '';
  @include ab(0, 50%, n, n);
  @include transition(all 0.6s linear);    
  @include transform(scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translateY(-150%));  
  width: 120%;
  height: 100px;
  z-index: 1;  
  background: rgba({{settings.light_color}}, 0.3);
  backface-visibility: hidden;
}
@mixin before-hover {
  top: 100%;
  @if $enable_rtl {
    right: -100%;
  }
  @else {
    left: -100%;
  }  
  @include transform(scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translateY(200%));
}
