        }
      }
    }   
    .content-square {
      li {
        position: relative;
        @include padding-start(30px);
        line-height: 24px;
        &:not(:last-child) {
          margin-bottom: 10px;
        }
        &:before {
          content: '';
          width: 8px;
          height: 8px;
          background: $main-color;
          @include opacity(0.8);
          @include ab(calc(50% - 4px), 0, n, n);
        }
      }      
    } 
    .flex-block {
      li {
        @extend %flexbox;
        @extend %align-items-center;
        @extend %justify-content-between;
        padding: 15px 0;                
        word-break: break-word;
        span:not(:first-child) {
          @include padding-start(15px);
          white-space: nowrap;
        }
        &:not(:last-child) {
          border-bottom: 1px solid rgba($body-border-color, 0.5);          
          margin-bottom: 0;
        }
      }
    }
    .content-img {
