			overflow-y: auto;
			overflow-x: hidden;
			@include padding-end(5px);
		}
		.cart-item-container::-webkit-scrollbar-track {
			@extend %webkit-scrollbar-track;
		}
		.cart-item-container::-webkit-scrollbar {
			@extend %webkit-scrollbar;
		}
		.cart-item-container::-webkit-scrollbar-thumb {
			@extend %webkit-scrollbar-thumb;
		}	
		.cart-item-info {
			@include padding-end(15px);
		}		
		.items-inner {
			@include translate(0, 100px);
			@extend %opacity-zero;			
			.cart-close {
				@include ab(0, n, n, 0);
			}
		}
		@for $index from 0 through 18 {
			.animated-#{$index}{
				@include transition(transform 1s cubic-bezier(0.165,.84,.44,1) calc(0.1s + calc(0.12s  #{$index})), opacity 1s cubic-bezier(.165,.84,.44,1) calc(0.2s + calc(0.12s  #{$index})) )
			}
		}		
	}	
	.cart-order-note {
		padding-top: 15px;
		span {
			display: block;
			padding-bottom: 5px;
			@extend %font-size-4;
		}
	}
	.wrap-action {
		@include ab(n, 30px, 15px, 30px);
		background: $popup-bg-color;
	}
	.total {
		@extend %body-title-color;
		> span:first {
			font-weight: 700;
		}
	}	
	.quantity {
		margin-top: 8px;
		.qty-wrapper {
			margin: 0;
			max-width: 45px;
			input {
				cursor: pointer;
			}
			.qty-inner {
				display: none;
			}
		}
	}
	.freeshipping-price {
		margin: 30px -30px -15px;
		padding: 12px;
		p {
			@extend %justify-content-center;
		}
	}
	&.opened {
		@extend %translate-zero;
		margin-top: 0 !important;
		opacity: 1;
		z-index: 9999999;
		.c-close {
			@extend %opacity;
		}
		.cart-info .items-inner {
			@extend %opacity;
			@extend %translate-zero;
		}
		.cart-content {
			opacity: 1;
			visibility: visible;
		}
	}
	.wrap-total {
		margin-top: 20px;
		.sb-progress {
			@include load_progressbar;
		}
		.sb-progress-bar {
			@include per_progressbar;
		}
		.sb-text {
			margin-top: 20px;
			@extend %body-title-color;
			span {
				display: inline-block;
			}
		}
	}
}
/* Back to top ---------------------------------*/
#scroll-to-top {
  @include transition(all 0.2s ease-out 0s);
  @include fixed(n, n, 10px, 15px);
  @extend %opacity-zero;
  -moz-backface-visibility: hidden;    
  z-index: 1000;  
  cursor: pointer;
  text-align: center; 
    &.on {
      bottom: 10%;
      @extend %opacity;
      @media (max-width: 991px) {
        bottom: 83px;
      }
    }
    i {
      display: none;
    }
    a {
      position: relative;
      width: 38px;
      height: 38px;      
      display: block;      
      padding: 10px;
      @extend %font-size-2;
      border: 1px solid $body-border-color;
      background: $body-background-color;
      @include transition(all 0.3s ease-in-out 0s);
      @extend %border-radius-circle;
      &:before {
        content: "\ea8a";                
        display: inline-block;    
        color: color-contrast($body-background-color);
        @extend %font-family;        
        @include transition(all 0.3s ease-in-out 0s);
        @extend %hover-icons-number;
      }      
    } 
  } 
