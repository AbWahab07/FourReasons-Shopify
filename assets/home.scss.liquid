@mixin text-shadow-ps {
  text-shadow:  0 0 10px #3498DB,
                0 0 20px #3498DB,
                0 0 30px #3498DB,
                0 0 40px #2980B9,
                0 0 70px #2980B9,
                0 0 80px #2980B9,
                0 0 100px #2980B9,
                0 0 150px #2980B9;
}
@mixin prefix($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else if $prefix == moz {
      -moz-#{$property}: $value;
    } @else if $prefix == ms {
      -ms-#{$property}: $value;
    } @else if $prefix == o {
      -o-#{$property}: $value;
    } @else if $prefix == spec {
      #{$property}: $value;
    } @else  {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}
@mixin transform-origin($origin) {
  @include prefix('transform-origin', $origin, moz ms webkit spec);
}
@mixin box-shadow($shadow...) {
  @include prefix('box-shadow', #{$shadow}, moz ms webkit spec);
}
@mixin transition($transition...) {
  @include prefix(transition, $transition, ms webkit spec);
}
@mixin transform($transform) {
  @include prefix(transform, $transform, ms webkit spec);
}
@mixin flex($value: 0 1 auto) {
  @include prefix(flex, $value, webkit moz ms spec);
}
@mixin flex-wrap($wrap) {
  @include prefix(flex-wrap, $wrap, webkit moz ms spec);
}
@mixin flex-direction($direction) {
  @include prefix(flex-direction, $direction, webkit moz ms o spec);  
}
@mixin flex-basis($width: auto) { 
  -ms-flex-preferred-size: $width;
  @include prefix(flex-basis, $width, webkit moz spec);
}
@mixin align-self($align: auto) { 
  -ms-flex-item-align: $align;
  @include prefix(align-self, $align, webkit spec);
}
@mixin flex-grow($grow) {
  @include prefix(flex-grow, $grow, webkit spec);
}
@mixin align-content($align: center) {
  @include prefix(align-content, $align, webkit ms spec);
}
@mixin justify-content($justify: flex-start) {
  @include prefix(justify-content, $justify, webkit ms spec);
}
@mixin align-items($value: stretch) {
  $alt-value: $value;
  @if $value == 'flex-start' {
    $alt-value: start;
  } @else if $value == 'flex-end' {
    $alt-value: end;
  }  
  -ms-flex-align: $alt-value;
  @include prefix(align-items, $value, webkit moz ms o spec);
}
@mixin translateX($x) {
  @if $enable_rtl {
    @include transform(translateX($x * (-1)));
  }
  @else {
    @include transform(translateX($x));
  }
}
@mixin translateY($y) {
  @include transform(translateY($y));
}
@mixin translate($x, $y) {  
  @if $enable_rtl {    
    @include transform(translate($x *(-1), $y));
  }
  @else {    
    @include transform(translate($x, $y));
  }
}
@mixin translate3d($x, $y, $z) {
  @include transform(translate3d($x, $y, $z));
}
@mixin skew($x, $y) {
  @include transform(skew(#{$x}deg, #{$y}deg));
}
@mixin scale($x, $y){
  @include transform(scale($x, $y));  
}
@mixin flexbox {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
}
@mixin inline-flex {
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -moz-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
}
@mixin border-radius($rd...) {
  border-radius: $rd;
  -moz-border-radius: $rd;
  -webkit-border-radius: $rd;
}
@mixin opacity ( $value )/*: 0.5 */ {
  opacity: $value;
}
@mixin ab($t, $l, $b, $r) {
  position: absolute;
  display: block;
  @if $enable_rtl {
    @if $l != n { right: $l; }
    @if $t != n { top: $t; }
    @if $r != n { left: $r; }
    @if $b != n { bottom: $b; }
  }
  @else{
    @if $l != n { left: $l; }
    @if $t != n { top: $t; }
    @if $r != n { right: $r; }
    @if $b != n { bottom: $b; }
  }
}
@mixin rotate($deg) {
  @include transform(rotate(#{$deg}deg));
}
@mixin rotateX($deg) {
  @include transform(rotateX(#{$deg}deg));
}
@mixin rotateY($deg) {
  @include transform(rotateY(#{$deg}deg));
}
@mixin placeholder {
  &::-webkit-input-placeholder {@content}
  &:-moz-placeholder           {@content}
  &::-moz-placeholder          {@content}
  &:-ms-input-placeholder      {@content}
}
@mixin before {
  content: '';
  @include ab(0, 50%, n, n);
  @include transition(all 0.6s linear);    
  @include transform(scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translateY(-150%));  
  width: 120%;
  height: 100px;
  z-index: 1;  
  background: rgba({{settings.light_color}}, 0.3);
  backface-visibility: hidden;
}
@mixin before-hover {
  top: 100%;
  @if $enable_rtl {
    right: -100%;
  }
  @else {
    left: -100%;
  }  
  @include transform(scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translateY(200%));
}
@mixin uppercase($upper) {
  @if $upper {
    text-transform: uppercase;
  }
  @else {
    text-transform: inherit;
  }
}
@mixin object-fit($a) {
