  .image {
    a {      
      overflow: hidden;      
      img {
        @extend %transition-ease-1;
        @extend %preserve-3d;
      }
    }
    &.bg-img {      
      @extend %background-option;  
      @extend %transition-ease-1;
      @extend %preserve-3d;    
      margin: 0;
    }
  }  
  &.text-box-overlay {   
    .text-container {
      display: table-cell;
      vertical-align: middle;       
    }   
    .text {            
      @extend %absolute-content;
      @extend %preserve-3d;
      z-index: 2;
    }
  }
  &.text-box-under_image {    
    .banner-content {     
      .subtitle {
        line-height: 24px;
      }
    }   
  }
  &.text-box-above_image {
    .text {
      overflow: hidden;      
    }
    .banner-content {     
      .subtitle {
        line-height: 25px;
      }
    }    
  }
  &.text-box-right_image {
    .banner-content {
      @extend %flexbox;   
      @media (max-width: 575px) {
        @include flex-direction(column);
      }   
      .image {
        @extend %flex;        
      }
      .text {
        @extend %flexbox;
        @extend %align-items-center;
        @media (min-width: 576px) {
          @include flex-box(33.333333%);          
        }
        @media (max-width: 575px) {
          display: block;
        }
        .text-container {
          @media (max-width: 991px) {
            zoom: 0.6;
          }
        }       
      }
    }
  }
  &.effect-2,
  &.effect-8 {
    .bg-img {
      @media (min-width: 1025px) {
        @include opacity(0.7);
      }
    }
    .image a:hover {
      @media (min-width: 1025px) {
        img {
          @include opacity(0.7);
        }        
      }
    }
  }
  &.effect-3,
  &.effect-4,
  &.effect-5,
  &.effect-6 {
    .banner-content {      
      @media (min-width: 1025px) {
        &:hover {
          .image a img {
            @include scale(1.15, 1.15);
          }   
          .bg-img {
            @include scale(1.05, 1.05);
          }
        }
      }          
    }
  }
  &.effect-3 {
    .banner-content {
      .text {
        @extend %transition-ease-1;
      }
      &:hover {
        @media (min-width: 1025px) {
          .text {
            margin-top: -5%;
          }
        }
      }
    }
  }
  &.effect-4 {
    .banner-content {
      .image a:before {
        @include before;
      }
      img {
        @extend %transition-ease-2;
      }
      &:hover {
        .image a {
          @media (min-width: 1025px) {
            &:before {
              @include before-hover;   
            }                  
          }
        }
      }
    }
  }
  &.effect-5 {
    .banner-content {
      .text, .title, .subtitle, .btn-group {
        @extend %transition-ease-1;
      }
      .text-container {
        @extend %transition-ease-1;
        @include scale(1, 1);        
      }
      @media (min-width: 1025px) {
        &:hover {
          .text-container {
            @include scale(1.05, 1.05);
          }
        }
      }          
    }
  }
  &.effect-6 {
    .banner-content {      
      .text {
        width: 100%;        
      }
      img, .text, .btn-group {
        @extend %transition-ease-4;
      }
      .btn-group {        
        @media (min-width: 1025px) {
          @include ab(n, 0, n, 0);
          @include translateY(40px);          
          @include opacity(0);
          visibility: hidden;
        }
      }
      @media (min-width: 1025px) {
        &:hover {
          .text {
            margin-top: -5%;
          }
          .btn-group {
            @include opacity(1);
            @include translateY(0);                      
            visibility: visible;
          }
        }
      }          
    }
  }
  &.effect-7 {
    .banner-content {
      a, img {
        @include transition(all .25s);
      }
      @media (min-width: 1025px) {
        &:hover {
          .image a img {
            -webkit-filter: grayscale(100%);
			      filter: grayscale(100%);
          }          
        }
      }          
    }
  }
  &.effect-8 {
    .banner-content {
      .text {
        @extend %transition-ease-1;
        @media (min-width: 1025px) {
          @include opacity(0);          
        }
      }
      @media (min-width: 1025px) {
        &:hover {
          .text {
            @include opacity(1);            
          }          
        }
      }          
    }
  }
}
