      &:hover {
        @media (min-width: 1025px) {
          .container{
            z-index: 4;
          }
        }
      }
      iframe {
        position: absolute;
        width: 100%;
        height: 100%;        
      }
    }
    .container-box {
      opacity: 0;
    }
    &.swiper-slide-active {
      @include opacity(1 !important);    
      visibility: visible;  
      .container-box {
        opacity: 1;
      }
    }
    > a  {
      display: block;
      width: 100%;
      height: 100%;
      position: relative;
      z-index: 1;      
      > img {
        &:not(.lazyload) {
          opacity: 0;
        }
      }
    }
  } 
