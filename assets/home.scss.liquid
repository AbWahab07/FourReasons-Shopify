        .item-title {
            padding-bottom: 25px;
            margin-bottom: 25px;
            position: relative;            
            &:after {
                content: '';
                height: 3px;
                width: 98px;
                background: $main-color;                
                @include ab(n, calc(50% - 49px), 0, n);
            }
