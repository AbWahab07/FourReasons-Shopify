                        .text {
                            @extend %flexbox;    
                            @extend %flex;
                            @extend %align-items-center;
                        }
                        .expand {
                            width: 49px;                       
                            text-align: center;
                            position: relative;
                            @extend %flexbox;
                        }
                   }
               }
            }
        }
    }
    .faq-style-1 {
        .faq-column {
            .faq-body {
              .faq-item-inner {
                 .faq-header-item {
                     a {                        
                        background: $body-title-color;
                        .text {                          
                            @extend %font-size-1;
                            font-weight: 700;
                            text-transform: uppercase;
                            color: color-contrast($body-title-color);
                            line-height: 1.2;    
                            padding: 5px 15px 5px 30px;  
                            min-height: 55px;                  
                        }
                        .expand {                           
                            background: $main-color;
                            color: $main-color-text;                           
                            &:after, &:before {
                                content: ""; 
                                background: $main-color-text;
                                @include ab(50%, 50%, n, n);
                                @include translate(-50%, -50%);
                            }
                            &:before {                                                       
                                width: 20px;
                                height: 2px;
                            }  
                            &:after {                         
                                width: 2px;
                                height: 20px;  
                                @extend %opacity-zero;                          
                            }                          
                        }                    
                        &.collapsed .expand:after {
                            @extend %opacity;   
                        }                                        
                     }
                 }
                 .faq-item-content {
                    padding: 14px 0 37px 30px;
                    line-height: 1.6;
                    @extend %font-size-1;
                    @media (max-width: 479px) {
                        @include padding-start(5%);
                    }
                 }
              }       
            } 
        }
    }  
    .faq-style-2 {
        .faq-body {
            border: 1px solid $body-border-color;
            .faq-item {
                &:not(:last-child) {
                    border-bottom: 1px solid $body-border-color;
                }
                .faq-item-inner {
                    padding: 0 30px;
                    @media (max-width: 479px) {
                        padding: 0 15px;
                    }
                }
                .faq-header-item {
                    > a {
                        padding: 15px 0 5px;
                        .text {
                            font-weight: 400;
                            @extend %font-size-1;
                            @extend %body-title-color;  
                        }
                        .expand {
                            @extend %justify-content-flex-end;
                            @include opacity(0.5);
                            &:before {
                                content: '\ea8a';
                                font-family: 'arenafont';
                            }
                        }
                        &.collapsed {
                            .expand:before {
                                content: '\e905';
                            }
                        }
                    }
                }
                .faq-body-item {
                    padding: 16px 0 30px;
                    border-top: 1px solid $body-border-color;
                    line-height: 1.7;
                    @extend %font-size-1;
                }
            }
        }
    }
    .row {
        > div:not(:last-child) {
            @media (max-width: 767px) {
                margin-bottom: 30px;
            }
        }
        .home_instagram_list {            
            &.row {
                margin-left: -12px;
                margin-right: -12px;
                > div {
                    padding: 10px 12px;
                    margin-bottom: 0;
                    > .instagram-hover {
                        top: 10px;
                        left: 12px;
                        right: 12px;
                        bottom: 10px;
                    }
                }
            }
        }
    }   
    .contact-form {
        input[type="text"],  input[type="email"], textarea {
            background: transparent;
            &:focus {
                border-color: inherit;
            }                        
            @include placeholder {
                color: inherit;
            }
        }
        .grid-form {
            .grid-form-name {
                @media(min-width: 992px) {
                    margin-left: -15px;
                    margin-right: -15px;
                    @include flexbox;
                    > div {
                        padding: 0 15px;
                        @include flex-box(50%);
                    }
                }
            }
        }
    } 
