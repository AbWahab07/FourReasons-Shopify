.lb-content {
    position: relative;
    .lb-pointer {
        @include ab(0, 0, 0, 0);
    }
}
.lb-pointer {    
    .pointer-item {
        position: absolute;          
        text-align: center;
        z-index: 99;
        &:hover {
            z-index: 991;
        }
        .pointer-icon {
            cursor: pointer;
            a {
                display: block;
                @media (min-width: 768px) {
                    pointer-events: none;                                   
                }                
                @media (max-width: 767px) {
                    z-index: 3;
                    padding: 10px;
                    @include ab(0, 0, 0, 0);     
                }
            }
        }  
        .product-item {
            visibility: hidden;               
            // position: absolute;
            position: fixed;
            z-index: 3;
            @extend %transition-ease;
            @extend %opacity-zero;
            @media (max-width: 767px) {
                display: none;
            }
        }  
        &.tooltip-top,
        &.tooltip-bottom {
            .product-item {
                left: 50%;                
                &:after {
                    content: '';
                    height: 1em;
                    @include ab(n, 0, n, 0);                    
                }
            }            
        }
        &.tooltip-top {
            .product-item {                
                top: 0;
                margin-top: -2em;    
                @include translate(-50%, -100%);            
                &:after {                    
                    bottom: -48px;
                }
            }
            &:hover {
                .product-item {
                    margin-top: -1em;
                }   
            }
        }    
        &.tooltip-bottom {
            .product-item {
                bottom: 0;
                margin-bottom: -2em;
                @include translate(-50%, 100%);  
                &:after {                    
                    top: -48px;
                }
            }
            &:hover {
                .product-item {
                    margin-bottom: -1em;
                }   
            }
        }    
        &.tooltip-right,
        &.tooltip-left {
            .product-item {
                top: 50%;                
                &:after {
                    content: '';
                    width: 1em;
                    @include ab(0, n, 0, n);
                }
            }
        }
        &.tooltip-right {
            .product-item {
                left: 0;
                @include margin-start(-2em);                
                @include translate(-100%, -50%);
                &:after {                    
                    right: -48px;
                }
            }
            &:hover {
                .product-item {
                    @include margin-start(-1em);
                }   
            }
        }    
        &.tooltip-left {
            .product-item {
                right: 0;
                @include margin-end(-2em);
                @include translate(100%, -50%);
                &:after {                    
                    left: -48px;
                }
            }
            &:hover {
                .product-item {
                    @include margin-end(-1em);
                }   
            }
        } 
        &:hover {            
            .product-item {
                visibility: visible;     
                position: absolute;              
                @include opacity(1);  
            }
        }   
    }
}
