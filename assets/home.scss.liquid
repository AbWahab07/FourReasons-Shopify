.item-style-3 {
    .row {
        @media (min-width: 1025px) {
            margin: 0 -22px;
            > div {
                padding: 0 22px;
            }
        }
    }
    .plan-item {        
        @extend %transition-ease;               
        .item-body {
            .list-minus {
                text-align: start;
            }
            .item-header-text {
                position: relative;                
                &:after {
                    content: '';
                    width: 100px;
                    height: 1px;
                    background: $main-color;
                    @include ab(n, calc(50% - 50px), 0, n);
                }
            }
        }
        &:hover {
            border-color: $body-border-color;
            @media (min-width: 1025px) {
                @include scale(1.15, 1.15);
                @include box-shadow(1px 1px 25px $body-border-color);
            }
        }
    }    
