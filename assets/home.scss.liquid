            @include flexbox;
            @include flex-direction(column);
            height: 100%;
          }
          span.image--style {
            padding: 0 !important
          }
        }
        img {        
          @media (min-width: 768px) {
            object-fit: cover;
            width: 100%;
            height: 100%;
          }
        }
      }
    }
    &.has-border {
      padding: 25px;
      border: 5px solid $main-color;
      @media (max-width: 479px) {
        padding: 5%;
      }
      .card-body {
        padding-bottom: 0;
      }      
      .product-card {     
        margin-bottom: 0;  
        .card-progress {
          padding-top: 0;
        }
      }
    }   
    &.spc-type-image {
      .big-content-body {        
        padding: 5%;
        @extend %absolute-content;
        @media (max-width: 479px) {
          width: 100%;
        }
        .b-title {          
          font-size: $size-h1;                    
          margin-bottom: 8px;
