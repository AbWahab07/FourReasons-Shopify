        .b-subtitle {
          font-size: $size-h2;                    
          line-height: 1;
          margin-bottom: 25px;
          @media (max-width: 575px) {
            margin-bottom: 5px;
          }
        }
        .b-content {          
          display: block;
          @media (max-width: 420px) {
            display: none;
          }
          ul, ol {
            list-style: none;
          }
          .list-dot {
            text-align: start;
            li {
              position: relative;
              line-height: 24px;
              @include padding-start(20px);
              &:before {
                content: '';
                width: 11px;
                height: 11px;
                background: $main-color;
                @extend %border-radius-circle;
                @include ab(calc(50% - 5px), 0, n, n);
              }
              &:not(:last-child) {
                margin-bottom: 10px;
              }
            }
          }
        }
        .btn-group {
          margin-top: 35px;
          @media (max-width: 575px) {
            margin-top: 8px;
          }
        }
      }
    }
    &.spc-type-slider {
      .spc-slider {
        padding-bottom: 0 !important;
        .owl-dots {
          bottom: 15px;
          .owl-dot {
            position: relative;
            margin-left: 5px;
            margin-right: 5px;
            &:after {
              content: '';
              border: 2px solid $light-color;
              @include ab(-2px, -2px, -2px, -2px);
              @extend %opacity-zero;
              @extend %border-radius-circle;
              @extend %transition-ease;
            }
            span {
              width: 15px;
              height: 15px;
              background: rgba({{settings.light_color}}, 0.2);    
              @extend %transition-ease;            
            }
            &.active {
              &:after {
                @extend %opacity;
              }
              span {
                @include scale(0.4, 0.4);
                background: $light-color;                  
              }
            }
            &:hover {
              @media (min-width: 1025px) {
                &:after {
                  @include opacity(1);
                }
                span {
                  @include scale(0.4, 0.4);
                  background: $light-color;                  
                }
              }
            }
          }
        }
        .owl-item {
          @extend %opacity-zero;
          &.active {
            @extend %opacity;
          }
        }
      }
    } 
    &.image-height-fill {
      margin-top: 25px;    
      @media (min-width: 768px) {
        @include flexbox;
        @include flex-direction(column);
        height: calc(100% - 42px);
      }
      img {        
        @media (min-width: 768px) {
          object-fit: cover;
          width: 100%;
          height: 100%;
        }
        @media (max-width: 767px) {
          position: static;
        }
      }
      span.image--style {
        @media (max-width: 767px) {
          padding-top: 0 !important;
        }
      }
      .spc-slider, .owl-stage, .owl-item, .spc-slider-item {
        @extend %flexbox;
        height: 100%;       
      }
      .owl-item, .spc-slider-item {
        @extend %flex-direction-column;
      }
      &.content-overlay,
      &.spc-type-image {
        span.image--style {
          @media (min-width: 768px) {
            padding: 0 !important;
          }
        }
        .big-content-head, a, .image--style {
          @media (min-width: 768px) {
            @include flexbox;
            @include flex-direction(column);
            height: 100%;    
          }                       
        }    
      }
      &.top-content {
        .big-content-body, a, .image--style {
          @media (min-width: 768px) {
            @include flexbox;
            @include flex-direction(column);
            height: 100%;    
          }      
        }
      }             
    }
  }
  &.has-border {
    .product-card .card-content {
      padding: 15px; 
      @media (max-width: 575px) {
        padding: 10px;
      }
    }
    .spc-block.image-height-fill {
