                @extend %body-light-color;
                display: inline-block;
                &:not(:last-child) {
                    @include padding-start(5px);
                }
            }
        }
        .product-name {
            font-size: $product-font-size;
            margin-bottom: 10px;
            font-weight: $product-title-weight;
        }
        .product-wrap-price {
            .price-label {
                display: none;
            }
        }  
        .product-button {
            margin-top: 30px;
            .btn {
                @extend %inline-flex;
                @extend %align-items-center;
                span + .icon-type {
                    @include margin-start(20px);
                    @include margin-end(0);    
                    width: 39px;
                    height: 39px;
                    text-align: center;
                    line-height: 39px;       
                    display: inline-block;
                    @extend %border-radius-circle; 
                }
            }
        }  
    }
    .main-slider {
        .product-header {
            padding-bottom: 20px;
        }
        .product-body {
            .product-name {
                font-size: $product-font-size + 5;
                line-height: 1.2;
            } 
            .product-price {
                font-size: $product-price-size + 6;
            }       
        }
    }
    .thumb-slider {
        .slick-slide {            
            @include padding-start(10px);
            @include padding-end(25px);
            padding-top: 20px;
            @media (min-width: 992px) and (max-width: 1199px) {
                padding-left: 0;
                padding-right: 0;
            }
            &:not(:last-child) {                                
                padding-bottom: 20px;
                border-bottom: 1px solid $body-border-color;                      
            }
        }
        .product-item-inner {
            @extend %flexbox;
            @extend %align-items-center;
            width: 100%;
            .product-header {
                @media (min-width: 992px) and (max-width: 1199px) {
                    @include flex-box(120px);
                    @include padding-end(20px);
                }
                @media (min-width: 1200px) {
                    @include flex-box(200px);
                    @include padding-end(30px);
                }
            }
            .product-body {
                @extend %flex;                
            }
        }
    }
}
.product-style-2 {
    .main-slider {
        .product-item-inner {
            @extend %flexbox;
            @extend %align-items-center;
            width: 100%;
            @media (max-width: 767px) {
                display: block;
            }
            .product-header, .product-body {
                @extend %flex;                
            }
            .product-header {
                @include padding-start(20px);
                @media (max-width: 767px) {
                    padding: 0 0 30px;
                }
            }
            .product-body {
                @include padding-start(50px);
                @media (max-width: 767px) {
                    @include padding-start(0);                   
                }
                .product-info {
                    @media (max-width: 767px) {
                        text-align: center !important;
                    }
                }
                .product-tag {
                    @extend %flexbox;
                    @extend %align-items-center;
                    @extend %flex-wrap;
                    margin-bottom: 10px;
                    @media (max-width: 767px) {
                        @include justify-content(center);
                    }
                    .tags > span {
                        text-transform: uppercase;
                        color: $main-color;
                        @extend %font-size-3;
                    }
                }
                .product-name {
                    font-size: $size-h2;
                    margin-bottom: 25px;
                }
                .card-sh-description {
                    @extend %font-size-3;
                    @extend %body-light-color;
                    line-height: 23px;                    
                    margin-bottom: 16px;
                }
                .product-wrap-price {
                    @extend %flexbox;
                    @extend %align-items-center;
                    @extend %flex-wrap; 
                    margin-bottom: 36px;
                    @media (max-width: 767px) {
                        @include justify-content(center);
                    }
                }
                .price-label {
                    text-transform: uppercase;
                    font-weight: 700;
                    font-size: $product-price-size + 4;
                    @include padding-end(25px);
                    line-height: 1;
                    @extend %body-title-color;                    
                }
                .product-price {
                    font-size: $size-h2;  
                    line-height: 1;                  
                } 
            }
        }
    }
    .thumb-slider {
        .product-slider-item {
            padding: 10px;
            margin-bottom: 10px;
            &:hover {
                @media (min-width: 1025px) {
                    background: $main-color;
                    .product-name a, 
                    .product-tag .tags > span,
                    .product-price, .product-price .price-sale {
                        color: $main-color-text;
                    }
                }
            }
        }
        .product-item-inner {
            @extend %flexbox;
            @extend %align-items-center;
            width: 100%;
            .product-header {
                @include flex-box(120px);
                @include padding-end(25px);
            }
            .product-body {
                @extend %flex;      
                .product-name {
                    margin-bottom: 9px;
                    a {
                        @include transition(none);
                    }
                }          
                .tags {
                    margin-bottom: 9px;
                    > span {
                        @extend %body-light-color;
                    }
                }
            }
        }
    }
}
.banner-tabs-content {
    .tab-image-text:empty {
        display: none !important;                 
    }
    .hp-tabs-list {   
        .tab-item .tab-content-inner > .tab-image {
            margin-top: 30px;
            @media (max-width: 575px) {
                margin-top: 15px;
            }
        }     
        &.no-padding {
            margin: 0;
            > div {
                padding: 0;                     
                .tab-content-inner > .tab-image {
                    margin-top: 0;  
                }         
            }
        }
    }
    .tabs-list-carousel {
        .owl-stage, .owl-item .tab-item {
            @extend %flexbox;
            @media (max-width: 767px) {
                display: block;
            }
        } 
        .tab-item {
            height: 100%;
            margin-top: 0;
        }
        .tab-content-inner {
            width: 100%;     
            @media (max-width: 767px) {
                display: block;
            }       
            &.tab-type-text {                
                .text-content-inner {
                    @extend %flexbox; 
                    @extend %align-items-center;
                    @extend %flex-wrap;
                    height: 100%;                                        
                }
            }   
            &.tab-type-image {
                .tab-item-image {
                    height: 100%;
                    width: 100%;
                    @extend %flexbox;                    
                    @extend %flex-direction-column;
                    @media (max-width: 767px) {
                        display: block;                            
                    }
                    .image--style {
                        height: 100%;
                        width: 100%;
                        @extend %flexbox; 
                        @media (max-width: 767px) {
                            display: block;                            
                        }
                        img {
                            height: 100%;
                            object-fit: cover;
                            font-family: "object-fit: cover";
                        }
                    }
                }
                .tab-item-style-1, .tab-item-style-2 {
                    height: 100%;
                    width: 100%;
                    @extend %flexbox;
                    @extend %align-items-center;
                    @extend %flex-wrap;
                    margin: 0 !important;
                    @media (max-width: 767px) {
                        display: block;                            
                    }
                    > div {
                        @include flex-box(100%);
                        padding: 0;
                    }                    
                }
            }         
        }               
    }    
    .nav-tab-wrapper .nav-tabs {
        border: none;
        &.text-center {
            @media (min-width: 768px) {
                @include justify-content(center);
            }   
            &.title-style-1,
            &.title-style-2,
            &.title-style-3 {
                .tab-title-image {
                    margin-left: auto;
                    margin-right: auto;
                }                
            }
        }  
        &.text-right {            
            @media (min-width: 768px) {
                @include justify-content(flex-end);
            }
            &.title-style-1,
            &.title-style-2,
            &.title-style-3 {                
                .tab-title-image {
                    @include margin-start(auto); 
                }               
            }
        }        
        li a {
            margin-bottom: 8px;
            padding: 5px 0;
            font-weight: 600;
            background: none;
            border: none;            
            @extend %font-size-1;
            @extend %border-radius-reset;
            .tab-title-image {
                margin-bottom: 10px;
                display: block;                
            }
        }        
        &.title-style-1,
        &.title-style-2,
        &.title-style-3 {
            .tab-title-image {
                @include flex-box(60px);
            }
        }
        &.title-style-2 {
            margin-bottom: 40px;
            li a {                
                @include opacity(0.7);                
                padding-top: 10px;
                padding-bottom: 5px;                
                &:hover {
                    @media (min-width: 1025px) {
                        @include opacity(1);
                        border-bottom: 2px solid currentColor;
                    }
                }
                &.active {
                    @extend %opacity;                    
                    border-bottom: 2px solid currentColor;
                }
            }
        }
        &.title-style-3 {
            border-bottom: 1px solid $body-border-color;
            padding-bottom: 8px;
            margin-bottom: 22px;
            @media (min-width: 768px) {                
                @include align-items(stretch);
                @include justify-content(center);
                li {
                    @include flex(1 1 auto);                    
                    @include align-self(flex-end);
                    @include justify-content(center);
                    max-width: 270px;
                    a {
                        &:after {
                            content: '';
                            border-left: 6px solid transparent;
                            border-right: 6px solid transparent;
                            border-bottom: 6px solid transparent;
                            @include ab(n, calc(50% - 3px), -20px, n);
                        }                       
                        &.active {
                            &:after {
                                border-top: 6px solid currentColor;
                            }
                        }
                    }
                }
            }
        }
        &.title-style-4 {
            margin-bottom: 50px;
            @media (max-width: 767px) {
                margin-bottom: 30px;
            }
            li {
                a {
                    width: 70px;
                    height: 70px;
                    padding: 0;
                    margin: 0 auto;
                    @extend %border-radius-circle;   
                    .tab-title-image {
                        padding: 15px;                                             
                    }
                    span {
                        width: 100%;
                        height: 100%;  
                        margin: 0;                                   
                    }                    
                    .image--style {
                        padding: 0 !important;      
                        img {
                            width: 100%;
                            position: static;                                                                          
                            display: inline-block; 
                        }           
                    }                    
                }
            }
        }
    }
    .tab-content-inner {
        @extend %flexbox;
        @extend %flex-direction-column;
        height: 100%;        
        .text-extended-inner {
            @extend %flexbox;
            @extend %flex-direction-column;
            @extend %justify-content-center;
            height: 100%;           
        }
        .text-extended {           
            > div:not(:last-child) {
                margin-bottom: 20px;
            }    
            .text-heading {
                font-weight: 900;
                line-height: 1.1;
            }    
            .text-subheading {
                line-height: 24px;
            }   
        }
        .text-content {
            line-height: 1.6;
            @extend %font-size-1;
        }
        .tab-item-style-1 {
            overflow: hidden;
            height: 100%;
            .tab-image-text {
                text-align: center;
                padding: 35px 10px;
                height: 100%;
                @extend %flexbox;
                @extend %flex-direction-column;
                .text-image {
                    margin-bottom: 35px;
                    .image--style {
                        padding: 0 !important;
                        img {
                            position: static;
                            width: auto;
                            margin: 0 auto;
                        }
                    }
                }
                .text-heading {
                    margin-bottom: 10px;
                    font-weight: 900;
                    font-size: $size-h4;
                }
                .text-subheading {
                    margin-bottom: 18px;
                    font-weight: 700;
                    @extend %font-size-2;
                }
                .text-content {
                    margin-bottom: 18px;
                    font-weight: 900;
                    @extend %font-size-3;
                }
            }
        } 
        .tab-item-style-2 {
            @extend %flexbox;
            @extend %align-items-center;
            margin-bottom: 35px;
            .tab-item-image {
                @include flex-box(110px);
                @include padding-end(35px);
                @media (max-width: 479px) {
                    @include padding-end(20px);
                }
            }
            .tab-image-text {
                @extend  %flex;
                .text-heading {
                    font-weight: 700;
                    font-size: $size-h4;
                    margin-bottom: 8px;
                }                
            }
        } 
        .tab-item-style-3 {           
            .text-heading {
                @extend .h2;
                line-height: 1.1;
                margin-bottom: 25px;
            }
            .text-subheading {
                @extend .h3;
                margin-bottom: 20px;                
                line-height: 1.1;
            }
            .text-content {
                line-height: 26px;
                @extend %font-size-1;
            }
            .tab-image-text + .tab-item-image {
                margin-top: 45px;
                @media (max-width: 767px) {
                    margin-top: 25px;
                }
            }
        } 
        .text-content {
            &:not(:last-child) {
                margin-bottom: 20px;
            }
            .text-heading {                
                margin-bottom: 8px;
                font-weight: 700;
                line-height: 1.1;
            }
            ul {
                list-style: none;
                margin-left: -15px;
                margin-right: 15px;
                @extend %flexbox;
                @extend %flex-wrap;                
                li {
                    color: inherit;
                    position: relative;
                    padding: 0 15px 0 40px;
                    margin-bottom: 5px;
                    @media (min-width: 992px) {
                        @include flex-box(50%);
                    }
                    @media (max-width: 991px) and (min-width: 768px) {
                        @include flex-box(100%);
                    }
                    @media (max-width: 767px) and (min-width: 576px) {
                        @include flex-box(50%);
                    }
                    @media (max-width: 575px) {
                        @include flex-box(100%);
                    }
                    &:before {
                        content: '';
                        width: 10px;
                        height: 2px;
                        background: $main-color;
                        @include ab(50%, 15px, n, n);
                        @include translateY(-50%);
                    }
                }
            }
        }     
    }    
}
.has-big-image {
    .container > .row,
    > .row {
        @extend %align-items-center;
        > div {
            @media (max-width: 767px) {
                &:not(:last-child) {
                    padding-bottom: 30px;
                }
            }
        }
    }
}
.home-banner-tabs {
    .tab-item-image {
        overflow: hidden;
        position: relative;
        img {
            @extend %transition-ease-1;
            @extend %preserve-3d;
        }
    }
    .img-hover-2 {
        .tab-item-image:hover {
            @media (min-width: 1025px) {
              img {
                @include opacity(0.7);
              }        
            }
        }
    }
    .img-hover-3,
    .img-hover-4,
    .img-hover-5 {
        .tab-item-image:hover {
            @media (min-width: 1025px) {
              img {
                @include scale(1.15, 1.15);
              }        
            }
        }
    }
    .img-hover-4 {
        .tab-item-image {
            &:before {
                @include before;
            }
            &:hover {
                @media (min-width: 1025px) {
                    &:before {
                      @include before-hover;   
                    }                  
                }
            }
            img {
                @extend %transition-ease-2;
            }  
        }           
    }
    .img-hover-7 {
        .tab-item-image {
            img {
                @include transition(all .25s);
            }
            @media (min-width: 1025px) {
                &:hover {
                    img {
                        -webkit-filter: grayscale(100%);
                        filter: grayscale(100%);
                    }         
                }
            }  
        }        
    }             
}

.s-banner-content {
    .row {
        @extend %align-items-center;        
    }
    .banner-item {
        margin-bottom: 0;
        .image {
            position: relative;
            z-index: 1;  
            max-width: inherit;       
            &.image-auto {
                .image--style {
                    padding: 0 !important;
                    text-align: center;
                    img {
                        position: static;
                        width: auto;
                        margin: 0 auto;
                    }
                }
            }   
        }
    } 
    .textbox-content {
        text-align: center;   
        .t-text-1, .t-text-2 {
            line-height: 1.6;
        }        
        .t-button {            
            .btn-sbanner {
                @extend %inline-flex;
                @extend %align-items-center;                
            }
            .icon-type {
                @include margin-end(14px);             
                line-height: 100%;
            }
        }
    }
    .button-group {            
        .btn-s-banner {                
            @extend %align-items-center;                
        }       
    }
}
