.wrap-plan-items {
    &.no-padding {
        margin: 0;
        > div {
            padding: 0;
        }
        .plan-item {
            margin-bottom: 0;
            height: 100%;
        }
    }
}
.plan-item {   
    margin-bottom: 30px; 
    position: relative;   
    // height: calc(100% - 30px);
    @extend %flexbox;
    @extend %flex-direction-column;       
    .item-header {
        .item-header-image {            
            &.img-auto {
                padding-bottom: 13px;
                .image--style {
                    padding: 0 !important;
                    img {
                        position: static;
                        width: auto;                        
                        display: inline;                        
                    }
                }
            }
        }   
        &.bg-image {
            background-repeat: no-repeat;
            &.bg-size-auto {
                background-size: auto;
            }
        }     
    }        
    .item-title {        
        sup {
            font-size: 62%;
            top: -1em;            
        }
        &:empty {
            display: none;
        }
    }
    .item-subtitle {
        line-height: 1.1;     
        sup {
            font-size: 40%;
            top: -1em;
            font-weight: 400;
        }
        sub {
            font-size: 16%;
            bottom: -0.5em;
            font-weight: 400;
            @include opacity(0.8);
        }   
    }
    .item-content {
        ul {
            list-style: none;
            li {
                position: relative;
                i {
                    color: $main-color;
                    margin-right: 9px;
                }                
            }
        }
        .dot-circle, .list-dot {
            li {
                @include padding-start(22px);
                &:before {
                    content: '';
                    width: 8px;
                    height: 8px;                   
                    @extend %border-radius-circle;
                    @include ab(calc(50% - 4px), 0, n, n);
                }
            }
        }
        .dot-circle {
            li {                                
                &:before {
                    @include box-shadow(inset 0 0 0 1px $main-color);                   
                }
            }
        }
        .list-dot {
            li {                                
                &:before {
                    background: $main-color;
                }
            }
        }
        .border-line {            
            li {
                padding-top: 20px;
                padding-bottom: 20px;
                &:not(:last-child) {                                        
                    &:after {
                        content: '';
                        height: 1px;
                        background: currentColor;
                        @include opacity(0.2);
                        @include ab(n, 0, 0, 0);
                    }
                }
            }
        }
        .list-minus {
            text-align: start;
            li {                
                padding: 2px 0 2px 25px;
                line-height: 22px;
                &:before {
                    content: '';
                    height: 2px;
                    width: 10px;
                    background: $main-color;
                    @include ab(calc(50% - 1px), 0, n, n);
                }
                &:not(:last-child) {
                    margin-bottom: 10px;
                }
            }
        }        
        .flex-block {           
            li {
                @extend %flexbox;
                @extend %align-items-center;
                line-height: 26px;
                text-align: start;
                &:not(:last-child) {
                    border-bottom: 1px solid $body-border-color;
                    padding-bottom: 14px;
                    margin-bottom: 14px;
                }
                span {
                    &:first-child {
                        @extend %flex;
                        font-weight: 300;
                        @include padding-start(35px);
                    }
                    &:last-child {
                        @include flex-box(41%);
                        font-weight: 700;
                        @include padding-start(10px);
                    }
                }
            }
        }
    }
    .item-price {
        sub {
            bottom: 0;
            background: inherit;
            color: inherit;
            font-size: 55%;
        }
    }
   .item-footer-inner {
       width: 100%;           
       &.d-flex {
            @extend %flexbox;
            @extend %flex-wrap;
            @extend %align-items-center;
            @extend %justify-content-between;               
       }
   } 
   &.bg-image { 
       background-repeat: no-repeat;
       background-size: cover;
        .item-footer-inner {
            padding: 0;
        }
        &.bg-size-auto {
            background-size: auto;
        }
   }   
}
.item-style-2 {
    .plan-item {  
        border: 12px solid #252525; 
        margin-top: 100px;         
        @extend %transition-ease;
        .item-header {
            position: relative;
            display: inline-block;
            @include ab(-90px, 38px, n, n);
            @media (max-width: 991px) and (min-width: 768px) {
                left: 25px;
            }
            .item-header-text {                
                padding: 0 2px;                
                sup {
                    font-size: 48%;
                    top: -1em;
                }
                sub {
                    font-size: 16%;
                    bottom: -0.5em;
                    @include opacity(0.3);
                }
            }
        }   
        .item-body {
            padding: 45px 40px 20px;    
            @media (max-width: 991px) and (min-width: 768px) {
                padding-left: 30px;
                padding-right: 30px;
            }               
            .item-footer-inner {
                padding-left: 40px;
                padding-right: 40px;
                @media (max-width: 991px) and (min-width: 768px) {
                    padding-left: 30px;
                    padding-right: 30px;
                }               
            }            
        }
        .item-footer-inner {
            padding: 0 40px 40px;
            @media (max-width: 991px) and (min-width: 768px) {
                padding-left: 30px;
                padding-right: 30px;
            }               
        }
        &:hover {
            @media (min-width: 1025px) {
                border-color: $main-color;
            }
        }
    }
