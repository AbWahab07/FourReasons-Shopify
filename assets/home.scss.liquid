/*calculate YIQ and return text color follow Background*/
@function color-contrast($color, $dark: $dark-color, $light: $light-color) {
  @if $color == null {
    @return null;
  }
  @else {
    $yiq: (red($color) * 299 + green($color) * 587 + blue($color) * 114) / 1000;
    @return if($yiq >= 128, $dark, $light);
  }
}
@function pxtorem($target, $context: $body-font-size) {
  @return ($target / $context) * 1rem;
}


.bc-wrapper {
  position: relative;
  z-index: 1;
  overflow: hidden;
  &.slide-height-full-screen {
    .home-slideshow {
      .swiper-slide {
        min-height: 100vh;
        > a {
          @include ab(0, 0, 0, 0);
        }        
      }
    }
  }   
  .swiper-button-white {
    @include ab(calc(50% - 25px), n, n, n);
    @extend %transition-ease;
    @extend %opacity;
    width: 50px;
    height: 50px;
    margin: 0;      
    text-align: center;
    outline: none;
    background-image: none;
    @media (max-width: 1024px) {
      width: 40px;
      height: 40px;
    }
    &:before {
      width: 100%;
      height: 100%;
      line-height: 50px;
      color: inherit;        
      @extend %font-small-icon;    
      @extend %font-family;        
      @media (max-width: 1024px) {
        line-height: 40px;
        font-size: $font-small-icon - 4px;
      }
    }  
    &.swiper-button-next {
      @media (min-width: 1025px) {
        @if $enable_rtl {
          left: -60px;
        }
        @else {
          right: -60px;
        }
      }
      @media (max-width: 1024px) {
        @if $enable_rtl {
          left: 0;
        }
        @else {
          right: 0;
        }
      }
      &:before {        
        @if $enable_rtl {
          content: '\ea89';
        }
        @else {
          content: '\ea8b';
        }  
      }
    }
    &.swiper-button-prev {
      @media (min-width: 1025px) {
        @if $enable_rtl {
          right: -60px;
        }
        @else {
          left: -60px;
        }        
      }
      @media (max-width: 1024px) {
        @if $enable_rtl {
         right: 0;
        }
        @else {
          left: 0;
        }
      }
      &:before {
        @if $enable_rtl {
          content: '\ea8b';
        }
        @else {
          content: '\ea89';
        } 
      }        
    } 
    &:hover {
      @media (min-width: 1025px) {
        @include opacity(0.7);
      }
    }  
    &.swiper-button-disabled {
      @extend %opacity-zero;
    }  
  }    
  &:not(.slide-height-auto) {
    .swiper-slide {
      .slider-layer {
        img {
          @include object-fit(cover);
        }     
      }      
    }
  }
  &.slide-height-full-screen {
    .swiper-slide {
      a>*{
        height: 100%;
      }
    }
  }
  .banner-item {
    margin-bottom: 0;
    @media (min-width: 768px) {
      @include padding-start(5px);        
      &:not(:last-child) {
        padding-bottom: 5px;
      }
    }     
    @media (max-width: 767px) {
      padding-top: 5px;
    }      
  }
  &:hover {
    @media (min-width: 1025px) {
      .swiper-button-white {
        &.swiper-button-prev {
          @if $enable_rtl {
            right: 0;
          }
          @else {
            left: 0;
          }         
        }
        &.swiper-button-next {          
          @if $enable_rtl {
            left: 0;
          }
          @else {
            right: 0;
          }
        }
      }
    }
  }  
  .gallery-thumbs {
    @include ab(n, 0, n, n);
    width: 100%;
    z-index: 2;
    @media (max-width: 767px) {
      z-index: -1;
    }
    .swiper-slide {        
      text-align: center;
      background: none;   
      cursor: pointer;               
      .image {              
        margin: 0 auto;        
      }      
      .thumb-text {
        font-size: $body-font-size - 1;
        text-transform: uppercase;      
        margin-top: 10px;    
        font-family: $font-heading; 
        &:empty {
          display: none;
        }
      }
    }
    .slide-index {
      @include flex-box(85px);
      padding-right: 20px;
      > span {
        display: block;        
        font-weight: 400;
        width: 65px;
        height: 65px;
        border-width: 2px;
        border-style: solid;
        line-height: 61px;
        text-align: center;
        font-size: 22px;
        @extend %border-radius-circle;
      }
    }
    .slide-text-content {
      .text-1 {
        font-family: $font-heading;
        line-height: 1.1;
        margin: 0 0 9px;
      }
      .text-2 {
        max-height: 40px;
        line-height: 20px;
        @include line-clamp(2);
      }
    }
  }
  .swiper-pagination {                           
    overflow: visible;
    width: auto;
    z-index: 10;
    .swiper-pagination-bullet {        
      width: 10px;
      height: 10px;       
      outline: none;
      font-size: 0;
      margin: 0 auto;
      position: relative;
      background: $main-color;
      @extend %border-radius-circle;
      @extend %transition-ease;
      @include opacity(0.5);           
      &.swiper-pagination-bullet-active {
        @extend %opacity;
      }
      &:hover {
        @media (min-width: 1025px) {
          @include opacity(1);
        }
      }     
      &.has-thumbnail {        
        text-align: center;
        background: none;           
        @extend %border-radius-reset;         
        img {
          width: auto;
        } 
        > span {
          display: block;                   
          &.thumb-text {
            font-size: $body-font-size - 1;
            text-transform: uppercase;      
            margin-top: 10px;    
            font-family: $font-heading; 
            &:empty {
              display: none;
            }
          }
        }
      }
    }           
  } 
  &.pagination-position_bottom-center, 
  &.pagination-position_bottom-left,
  &.pagination-position_bottom-right {  
    .swiper-pagination {      
      @extend %align-items-center;                             
      white-space: nowrap;           
    }
    .gallery-thumbs {
      @extend %flexbox;
      width: 100%;  
    }    
  } 
  &.pagination-position_bottom-center {
    .swiper-pagination,
    .gallery-thumbs {
      @include translate(-50%, 0);
      @include ab(n, 50%, 15px, n);  
      @extend %justify-content-center;
      
    }
    .gallery-thumbs {
      bottom: 70px;      
      @media (max-width: 767px) {
        bottom: 15px;
      }
    }
  }
  &.pagination-position_bottom-left {
    .swiper-pagination,
    .gallery-thumbs {
      @include ab(n, 50px, 15px, n);  
    }
    .gallery-thumbs {
      bottom: 70px;      
      @media (max-width: 767px) {
        bottom: 15px;
      }
    }
  }
  &.pagination-position_bottom-right {
    .swiper-pagination,
    .gallery-thumbs {
      @include ab(n, n, 70px, 50px);  
      @extend %justify-content-flex-end;
    }
    .gallery-thumbs {
      bottom: 70px;      
      @media (max-width: 767px) {
        bottom: 15px;
      }
    }
  }
  &.pagination-position_middle-left .swiper-pagination,
  &.pagination-position_middle-right .swiper-pagination {
    @include translateY(-50%);
    .swiper-pagination-bullet {
      display: block;
      position: relative;
      &.has-thumbnail img {
        @include object-fit(contain);        
        @include ab(0, 0, n, n);
      }
    }
  }
  &.pagination-position_middle-left .swiper-pagination {
    @include ab(50%, 30px, n, n);    
  }
  &.pagination-position_middle-right .swiper-pagination {
    @include ab(50%, n, n, 30px);    
  }
  &.navigator-style-2 .swiper-pagination .swiper-pagination-bullet {    
    @include opacity(1);    
  }  
  &.navigator-style-4 .swiper-pagination .swiper-pagination-bullet,
  &.navigator-style-6 .swiper-pagination .swiper-pagination-bullet {
    &:after {
      content: '';
      width: 100%;
      height: 100%;      
      @extend %border-radius-circle;
      @extend %opacity-zero;
      @include ab(0, 0, n, n);        
    }
    &:hover {
      @media (min-width: 1025px) {  
        &:after {
          @include opacity(1);
          @include scale(0.4, 0.4);         
        } 
      }
    }
    &.swiper-pagination-bullet-active {       
      &:after {
        @extend %opacity;       
        @include scale(0.4, 0.4);
      } 
    }
  }  
  &.navigator-style-5 .swiper-pagination .swiper-pagination-bullet {    
    @include opacity(1);
    &:after {
      content: '';
      width: 3px;
      height: 3px;      
      background: $light-color;
      @extend %border-radius-circle;      
      @include ab(50%, 50%, n, n);        
      @include translate(-50%, -50%);
    }
    &:hover {
      background: $light-color !important;      
    }
    &.swiper-pagination-bullet-active {
      background: $light-color !important;         
    }
  }
  &.navigator-style-6 .swiper-pagination .swiper-pagination-bullet {
    @include scale(0.6, 0.6);
    &:hover {
      @media (min-width: 1025px) {
        @include scale(1, 1);  
      }
    }
    &.swiper-pagination-bullet-active {      
      @include scale(1, 1);
    }
  }
  &.navigator-style-7 .swiper-pagination .swiper-pagination-bullet {
    @include border-radius(0);
  }
  &.navigator-style-8 .swiper-pagination .swiper-pagination-bullet {
    background: none !important;
    font-size: $size-h4 + 1px;        
    line-height: 100%;
    font-weight: 700;    
    font-family: $font-heading;
  }
  &.content-overlay--disabled {    
    .home-slideshow {
      .swiper-slide {
        @extend %flex-direction-column;
      }
      .swiper-content {
        position: static;
        .container-box {
          position: static;
          transform: translate(0, 0) !important;
          width: auto;
          display: block;
        }
      }
    }
  }
}  
