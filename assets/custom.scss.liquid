.slide-wrapper {
  padding: 150px 0;
  
  .slick-prev,
  .slick-next {
  	position: absolute;
    display: block;
    width: 33px;
    height: 33px;
    font-size: 0px;
    line-height: 0px;
    transform: translate(0, -50%);
  }
  
  .slick-prev {
    left: 10%;
    top: 60%;
  	background:url({{ 'icon-left-arrow.svg' | asset_url }});
    background-position: center;
    background-repeat: no-repeat;
    border: 1px solid #FFFFFF;
    border-radius: 50%;
    padding: 33px;
    z-index: 1;
  }
  
  .slick-next {
    right: 10%;
    top: 60%;
  	background:url({{ 'icon-right-arrow.svg' | asset_url }});
    background-position: center;
    background-repeat: no-repeat;
    border: 1px solid #FFFFFF;
    border-radius: 50%;
    padding: 33px;
    z-index: 1;
  }
}

