/*============================================================================
 *  Feature : Global                                                         *
 *  Type : StyleSheet & Variables                                            *
 *  Global variables for custom design break point                           *
=============================================================================*/
$design-screen-large: 1440px;
$design-screen-medium: 1200px;
$design-screen-interm: 991px;
$design-screen-small: 768px;

$large: 'large';
$medium: 'medium';
$interm: 'interm';
$small: 'small';

$grid-breakpoints: (
  $small '(max-width: #{$design-screen-small})',
  $interm '(max-width: #{$design-screen-interm})',
  $medium '(max-width: #{$design-screen-medium})',
  $large '(min-width: #{$design-screen-large + 1})'
);
@mixin media-query($media-query) {
  $breakpoint-found: false;

  @each $breakpoint in $grid-breakpoints {
    $name: nth($breakpoint, 1);
    $declaration: nth($breakpoint, 2);

    @if $media-query == $name and $declaration {
      $breakpoint-found: true;

      @media #{$declaration} {
        @content;
      }
    }
  }

  @if $breakpoint-found == false {
    @warn 'Breakpoint "#{$media-query}" does not exist';
  }
}

