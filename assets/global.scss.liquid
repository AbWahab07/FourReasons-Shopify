  color:#d02e2e;
  background-color:#fff6f6;
}
.alert-success {
  border-radius: 0;
  border: 1px solid $body-border-color;
  background: none;
  padding: 12px 19px 12px 44px;
  position: relative; 
  @extend %font-size-1; 
  @extend %body-general-color;
  @media (max-width: 575px) {
    font-size: $body-font-size;
    @include padding-end(15px);
    @include padding-start(35px);
  }
  &:before {
    content: '';
    height: 2px;
    background: $main-color;
    @include ab(-1px, -1px, n, -1px);
  }
  &:after {
    content: '\ea92';
    color: #2e7d32;
    @extend %font-family;
    @include ab(50%, 20px, n, n);
    @include translateY(-50%);
    @media (max-width: 575px) {
      left: 15px;
    }
  }
  p {
    color: inherit;    
    font-size: inherit;
    @extend %flex;
  }
  .close {
    font-size: 0;
    @include padding-start(5px);
    &:before {
      content: "\e90a";
      font-family: 'arenafont';                     
      color: inherit;          
      @extend %font-size-2;
    }
  }
}
