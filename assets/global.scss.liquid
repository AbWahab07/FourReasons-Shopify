@mixin itemRange($pe){
  -webkit-box-flex: 0;
  -ms-flex: 0 0 #{$pe};
  flex: 0 0 #{$pe};
  max-width: #{$pe};
}
@mixin colWidth($i){
  $percent: 100/$i+'%';
  .col-#{$i}th{@include itemRange($percent)}
  @media (min-width: 576px) {
    .col-sm-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 768px) {
    .col-md-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 992px) {
    .col-lg-#{$i}th{@include itemRange($percent)}
  }
  @media (min-width: 1200px) {
    .col-xl-#{$i}th{@include itemRange($percent)}
  }
}
@for $i from 7 to 13{@include colWidth($i)}

