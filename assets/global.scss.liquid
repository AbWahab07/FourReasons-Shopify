      background-image: url({{ settings.desk_bg_image | img_url: '1920x' }});
      background-position: {{settings.desk_bg_position }};    
      background-size: {{ settings.desk_bg_size }};    
      background-repeat: {{ settings.desk_bg_repeat }}; 
    {% endif %}    
  }
}
.h-desk-absolute {
  @media (min-width: 992px) {
    @include ab(n, 0, n, n);
    z-index: 99992;
    width: 100%;      
    background: none;
    #topbar {
      position: relative;      
      @if $bg-top-bar != rgba(0, 0, 0, 0) {
        background: rgba($bg-top-bar, ($top-bg-transparent / 100));  
      } 
      .topbar {
        color: $text-top-bar-overlay;
        .tb-text.custom-text i {
          color: inherit;
        }
      }
      .tb-social .widget-social-icons li a {
        color: $icons-top-bar-overlay;
        &:hover {
          color: $icons-hover-top-bar-overlay;
        }
      }
    }
    #header-style {     
      @if $header-bg != rgba(0, 0, 0, 0) {   
        background: rgba($header-bg, ($header-bg-transparent / 100));       
      }
    }
    .header-inner {      
      @if ($header-bg-transparent / 100) < 1 {
        background-image: none;
        background: none;
      }
    }
    .horizontal-menu .menu-list > li {
      .search-icon > a,
      > a {
        color: $overlay-nav-link-color;        
      }
      &:hover, &.active {
        .search-icon > a,
        > a {
          color: $overlay-nav-link-hover-color;
        }
      }
    }   
    .header-content.nav-style-3 .horizontal-menu .menu-list > li:not(:first-child) > a:before,
    .header-content.nav-style-6 .horizontal-menu .menu-list > li > a:after, 
    .header-content.nav-style-7 .horizontal-menu .menu-list > li > a:after {
      background-color: $overlay-nav-link-hover-color;
    }
    .header-content.nav-style-4 .horizontal-menu .menu-list > li.active > a, 
    .header-content.nav-style-4 .horizontal-menu .menu-list > li:hover > a {
      border-color: $overlay-nav-link-hover-color;
    }
    .header-content.nav-style-5 .horizontal-menu .menu-list > li.active > a, 
    .header-content.nav-style-5 .horizontal-menu .menu-list > li:hover > a {
      background-color: rgba($overlay-nav-link-hover-color, 0.13);
    }
    .header-content.nav-style-8 .horizontal-menu .menu-list > li:not(:last-child) > a:after, 
    .header-content.nav-style-8 .horizontal-menu .menu-list > li:not(:last-child) > a:after, 
    .header-content.nav-style-10 .horizontal-menu .menu-list > li:not(:last-child) > a:after, 
    .header-content.nav-style-10 .horizontal-menu .menu-list > li:not(:last-child) > a:after {
      color: $overlay-nav-link-hover-color;
    }
    .header-content.nav-style-9 .horizontal-menu .menu-list > li.active > a, 
    .header-content.nav-style-9 .horizontal-menu .menu-list > li:hover > a {
      background-color: rgba($overlay-nav-link-hover-color, 0.22);
    }
    .header-icons ul {
      color: $header-overlay-icons-color;
      li {
        &:not(:first-child) {
          @if $header-overlay-icons-border != rgba(0, 0, 0, 0) {
            @include padding-start(30px);  
          }
          @else {
            @include padding-start(15px);
          }
          @media (min-width: 992px) {
            &:before {
              background: $header-overlay-icons-border;
            }
          }
        }
        > div,
        .cart-target {                
          > a {                   
            color: $header-overlay-icons-color !important;    
            .number {                         
              @if $header-overlay-icons-number-bg != rgba(0, 0, 0, 0) {          
                background: $header-overlay-icons-number-bg;     
                color: $header-overlay-icons-number-color;  
                font-size: 10px;
              }
              @else {
                @extend %font-size-2;
              }
            }  
            .icon-text {
              @include uppercase($navigation-uc);
            }               
            .icon-type {
              color: inherit;
              i {             
                color: inherit;
                background: $header-overlay-icons-bg;              
              }
            }          
            &:hover {
              @media (min-width: 1025px) {
                color: $header-overlay-icons-hover !important;
                .icon-type i {
                  background: $header-overlay-icons-hover-bg;
                }
              }
            }
          }
        }       
      } 
    } 
  }                 
}
.select-theme-chosen {
  z-index: 999;
}
.header-icons {   
  > ul {
    list-style: none;
    color: $header-icon-color;    
    @extend %flexbox;      
    @extend %align-items-center;       
    > li {
      display: inline-block;     
      cursor: pointer;   
      &:not(:first-child) {
        @if $header-icons-border != rgba(0, 0, 0, 0) {
          @include padding-start(30px);  
        }
        @else {
          @include padding-start(15px);
        }
        @media (min-width: 992px) {
          position: relative;
          &:before {
            content: '';
            height: 20px;
            width: 1px;
            background: $header-icons-border;
            @include ab(50%, 14px, n, n);
            @include translateY(-50%);
          }
        }
        @media (max-width: 767px) {
          @include padding-start(10px);
        }
      }
      &:empty {
        display: none;
      }
      .topbar-config .demo-icon {
        color: inherit;
      }
      > div,
      .cart-target {                
        > a {         
          @extend %flexbox;      
          @extend %align-items-center;
          border-radius: $header-icon-rd;
          color: $header-icon-color;     
          min-height: 30px;           
          @media (min-width: 992px) {            
            background: $header-icons-bg;
            padding: $header-icon-padding;  
            @if $header-icons-bg != rgba(0, 0, 0, 0) {
              .number {
                @include ab(calc(50% - 8px), n, n, -3px);
              }
            }
          }   
          svg {
            fill: $header-icon-color;
            color: $header-icon-color;
          }
          > span {
            @extend %flexbox;      
            @extend %align-items-center;   
            color: inherit;
          }  
          .number, .icon-text {
            @include padding-start(6px);
          } 
          .number {      
            @extend %border-radius-circle;
            @extend %justify-content-center;           
            @include margin-start(3px);
            width: 16px;
            height: 16px;
            padding: 0;                       
            font-size: 10px;
            text-align: center; 
            > span {
              font-size: inherit;
            }
            @media (min-width: 992px) {              
              @if $header-icon-bg != rgba(0, 0, 0, 0) {                
                background: $header-icon-bg;     
                color: $header-number-icons-bg-color;                                                                 
              }
              @else {
                font-size: $body-font-size - 2;            
              }
            }
          }  
          .icon-text {
            @include uppercase($navigation-uc);
          }               
          .icon-type {
            color: inherit;
            i {              
              color: inherit;                            
              @extend %flexbox;              
              @media (min-width: 992px) {
                font-size: $header-icon-font-size;                
              }
              @media (max-width: 991px) {
                font-size: $header-font-mobile;
              }
            }
          }
          .n-item {            
            @extend %font-size-2;
          }
          &:hover {
            @media (min-width: 1025px) {
              color: $header-icon-hover-color;
              background: $header-icons-hover-bg;
              svg {
                fill: $header-icon-hover-color;
                color: $header-icon-hover-color;
              }              
            }
          }
        }
      }
      &.li-wishlist,
      &.li-compare {
        > div > a {
          @media (min-width: 992px) {                        
            @if $header-icons-bg != rgba(0, 0, 0, 0) {             
             @include margin-end(19px);
            }
          }   
        }
        .icon-text {
          display: none;
        }
      }
      &.li-cart {
        .cart-target  {
          height: 100%;
          width: 100%;   
          > a {
            @media (min-width: 992px) {                        
              @if $header-icons-bg != rgba(0, 0, 0, 0) {             
               @include margin-end(19px);
              }
            }   
          }       
          .cart-dd {   
            color: $body-general-color;         
            &:before {
              content: '';
              height: 35px;
              @include ab(-15px, 0, n, 0);
            }
          }
          &:hover {
            @media (min-width: 1025px) {
              > .basket {
                color: $header-icon-hover-color;
              }
              .cart-dd {
                visibility: visible;
                z-index: 999;                   
                @include translate(0, 0);
                @include opacity(1);
              }
            }
          }                  
          &.opened {
            .cart-dd {
              visibility: visible;
              z-index: 999;
              @extend %opacity;
              @extend %translate-zero;
            }
          }
        }        
      }
    }
  }
  .navbar-responsive-menu {
    padding: 10px 0 10px 5px;
  }
}
