.title-wrapper {
  margin-bottom: 35px;
  position: relative;
  z-index: 1;
  word-break: break-word;
  @media (max-width: 767px) {
    margin-bottom: $padding-inset-md;
  }
  .title {
    @include uppercase($section-uc);   
    position: relative;         
  }
  .subtitle {    
    padding-top: 10px;        
    line-height: 1.2;            
  }
  .subtitle-2 {
    padding-top: 5px;        
    line-height: 1.3;         
  }
}
.section-heading {      
  .title, .subtitle, .subtitle-2 {
    padding: 0;
  }
  .title, .subtitle {
    font-family: $font-heading;
  }
  .title {    
    color: $section-heading-color;    
  }
  .subtitle {
    color: $section-subheading-color;    
  }
  .subtitle-2 {
    color: $section-subheading2-color;    
    line-height: 1.6;
  }
  &.style-1 {    
    @extend %flexbox;
    @extend %align-items-center;
    @extend %flex-wrap;   
    @media (max-width: 479px) {
      display: block;
    }
    .title {
      display: inline-block;      
      &:after {
        content: '';
        display: block;
        height: 12px;
        width: 105%;
        background: $section-border-color;
        margin: -15px 0 0 -3%;
      }
    }
    .subtitle {
      @include margin-start(auto);
      width: 68px;
      height: 68px;
      border: 2px solid currentColor;
      text-align: center;
      padding: 12px 5px;
      line-height: 40px;
      position: relative;
      @extend %border-radius-circle;
      @media (max-width: 479px) {
        @include margin-end(auto);
        margin-top: 10px;
      }
      &:before {
        content: '';
        width: 8px;
        height: 8px;
        background: $section-border-color;
        @extend %border-radius-circle; 
        @include ab(10px, n, n, 0);
      }
    }
    .subtitle-2 {
      @include flex-box(100%);
    }
  }
  &.style-2 {    
    &:after {      
      content: '';
      width: 100px;
      height: 1px;
      background: $section-border-color;
      display: inline-block;
      margin-top: 25px;
    }
  }
  &.style-3 {  
    .title {      
      &:after {        
        content: '';
        width: 114px;
        height: 1px;
        background: $section-border-color;
        display: block;
        margin: 14px 0 25px;        
      }
    }
    &.text-center .title:after {
      margin-left: auto;
      margin-right: auto;      
    }
    &.text-right .title:after {
      @include margin-start(auto);      
    }
  }
  &.style-4 {    
    .title {
      @media (min-width: 575px) {
        padding: 0 115px;
        display: inline-block;
        &:before, &:after {
          content: '';
          height: 1px;
          width: 80px;
          background: $section-border-color;
          position: absolute;
          top: 50%;
          @include translate(0, -50%);
        }
        &:before {
          left: 0;
        }
        &:after {
          right: 0;
        }
      }      
    }
  }
  &.style-5,
  &.style-6 {           
    @extend %flexbox;
    @extend %align-items-center;
    @extend %flex-wrap;    
    .title {      
      &:after {
        content: '';
        height: 2px;        
        display: block;                
      }
    }
    .subtitle {      
      padding: 0;
      line-height: 1.1;
      @include uppercase($section-uc);   
    }
    .subtitle-2 {
      @include flex-box(100%);
    }
    &.text-center {
      @extend %justify-content-center;
      .title {
        text-align: left !important;
      }
    }
    &.text-right {
      @extend %justify-content-flex-end;
      .title {
        text-align: left !important;
      }
    }
  }
  &.style-5 .title {
    @extend %flexbox;
    @extend %align-items-center;
    @extend %flex-wrap;
    &:after {       
      width: 64px;             
      margin-left: 30px;
      margin-right: 30px;
      background-image: url({{"separator.png" | asset_url}});
      background-repeat: repeat-y;
      background-position: center;
    }
  }
  &.style-6 {    
    .title {
      font-size: 76px;
      line-height: 76px;
      &:after {       
        width: 176px;           
        position: relative;
        bottom: 14px;
        left: 26px;
        background: $section-border-color;
      }
    }
  }
  &.style-7 {
    .subtitle {
      @include ab(0, 0, n, n);            
      white-space: nowrap;     
    }
    .title {
      padding-top: 40px;
      margin-bottom: 10px;      
      position: relative;
      z-index: 1;
      @media (max-width: 479px) {
        padding-top: 0;
      }
    }
    .subtitle-2 {
      position: relative;
    }
    &.text-center .subtitle {
      left: 50%;
      @include translateX(-50%);
    }
    &.text-right .subtitle {
      left: auto;
      right: 0;
    }
  }
  &.style-8 {    
    position: static;
    .title {
      padding-top: 30px;    
      display: inline-block;  
      &:before {
        content: '';
        width: 92px;
        height: 7px;      
        background: $section-border-color;
        @include ab(0, -45px, n, n);
        @media (max-width: 1245px) {
          left: 0;
        }
      }
    }
    .subtitle-container {      
      @include ab(50%, -150px, n, n);      
      @include transform(translate(0, -50%) rotate(270deg));  
      text-align: center;      
      .subtitle {
        margin-bottom: 15px;
      }
      .subtitle-2 {
        margin-bottom: 30px;
        position: relative;        
        display: inline-block;
        &:after, &:before {
          content: '';
          width: 51px;
          height: 7px;
          background: $section-border-color;
          @include ab(50%, n, n, n);
          @include translateY(-50%);
        }
        &:before {
          left: -80px;
        }
        &:after {
          right: -80px;
        }
      }
    }
  }
  &.simg-style-6 {
    .section-heading-img {
      display: inline-block;
      max-width: 100%;             
    }
  }
  &.text-center .section-heading-img .section-img-inner {
    margin-left: auto;
    margin-right: auto;    
  }
  &.text-right .section-heading-img .section-img-inner {
    @include margin-start(auto);
  }
}
