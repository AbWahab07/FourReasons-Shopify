.navbar-vertical-collapse > div {
  cursor: pointer;
}
/* Vertical Menu */
.vertical-menu {  
  padding: 0 !important;
  .dropdown {
    .btn {
      border: none;
      border-left: 1px solid $vertical-border-left-right;
      border-right: 1px solid $vertical-border-left-right;
      background: $vertical-bg-vertical;
      color: $vertical-menu-color;
      text-transform: none;
      width: 100%;    
      text-align: start;      
      position: relative;
      line-height: 20px;
      padding: 17px 45px 17px 55px;
      white-space: normal;
      font-size: $nav-font-size;   
      font-weight: $nav-weight;
      @include uppercase($navigation-uc);
      @extend %border-radius-reset;
      @extend %justify-content-flex-start;
      &:after {
        @extend %font-family;
        @extend %transition-ease;
        @include ab(calc(50% - 10px), n, n, 30px);        
        font-size: $body-font-size - 4;
        width: 12px;
        height: 20px;
        line-height: 20px;
        text-align: center;
        content:"\eaa4";        
      }
      &:before {
        content: "\e936";
        @extend %font-family;
        @extend %font-size;
        @include ab(calc(50% - 8px), 30px, n, n);
      }
    }
    .menu-list {
      width: 100%;
      margin: 0;
      z-index: 5;
      padding: 15px 0;
      background: $body-background-color;
      border: 1px solid $border-vertical-menu;
      border-top: none;
      z-index: 991;
      @extend %border-radius-reset;
      li {
        padding: 0 30px;
        @extend %flexbox;
        @extend %align-items-center;
        a {
          @extend %flex;
          @extend %flexbox;
          @extend %align-items-center;
        }
        .expand {
          width: 20px;
          text-align: end;
          color: $nav-dropdown-link-color;   
          @include opacity(0.7);
          &:before {
            @extend %before-expand;
          }
        }
      }
      > li {        
        @extend %body-title-color;       
        > a {
          padding: 10px 0;          
          color: inherit;
          font-weight: $nav-weight;
          font-size: $nav-font-size;     
          @include uppercase($navigation-uc);   
          > .cf-metafields--menu-icon {
            max-width: 50px;
            margin-right: 15px;
            display: inline-block;
          }
        }         
        > .expand {
          color: inherit;          
        }      
        &:hover {
          @extend %main-color;
        }
        &.dropdown {
          position: static;
          .dropdown-menu {            
            background: $nav-dropdown-background;          
            z-index: -99;            
            font-size: $nav-sub-font-size;     
            visibility: hidden;  
            min-height: 100%;        
            background-size: cover;
            max-width: calc(100vw - 320px);
            @include translate(10px, 0);
            @extend %opacity-zero;            
            @include ab(0, 100%, n, n);
            @include transition(all 0.45s ease-out 0s);
            .back-prev-menu {
              display: none;
            }
            li {              
              a {
                padding: 8px 0;
                color: $nav-dropdown-link-color;                
                span {
                  color: inherit;
                }
                &:hover {
                  color: $nav-dropdown-link-hover-color;
                }
              }
              &:hover {
                > .dropdown-menu {
                  visibility: visible;
                  z-index: 9999;
                  @extend %opacity;
                  @extend %translate-zero;
                }
              }
            }
          }
          &:hover,
          &.menu-showing--active {
            > .dropdown-menu {
              visibility: visible;
              z-index: 9999;
              @extend %opacity;
              @extend %translate-zero;
            }
          }
        }        
      }
    }
    &.show {
      .btn {
        &:after {
          @include rotate(180);          
        }
      }
    }
  }
}
/* Horizontal Menu */
.horizontal-menu {
  position: static;  
  .back-prev-menu {
    display: none !important;
  }
  .menu-list {
    @extend %flexbox;
    @extend %align-items-center;
    @extend %flex-wrap;
    &.text-center {
      @extend %justify-content-center;
    }
    &.text-right {
      @extend %justify-content-flex-end;
    }
    > li {      
      @include border-inline-end(1px solid $nav-border-color);
      .search-icon > a,
      > a {
        position: relative;         
        color: $nav-link-color; 
        font-weight: $nav-weight;
        font-size: $nav-font-size;           
        letter-spacing: {{ settings.nav_lsp | default: '0px' }};
        @extend %flexbox;
        @extend %align-items-center;
        @include uppercase($navigation-uc);
        > .cf-metafields--menu-icon {
          max-width: 50px;
          display: inline-block;
          margin-right: 5px;
        }
        > span {
          color: inherit;
          font-size: inherit;
          &:first-child {
            position: relative;
          }
        }
        .icon-type {
          i {
            @extend %font-small-icon; 
            &:before {
              height: 1em;
            }
          }                   
        }
      }     
      .search-icon > a {
        max-height: 26px;
        line-height: 26px;
        > span:not(:first-child) {
          @include padding-start(6px);
        }
      }
      &.dropdown {
        > a > span:first-child,
        > a > span.cf-metafields--menu-icon + span {
          @extend %flexbox;
          @extend %align-items-center;
          &:after {
            content: "\e905";
            font-size: 8px;
            color: inherit;                     
            @include opacity(0.6);
            @include padding-start(5px);
            @extend %font-family; 
          }
        }
        > a > span.cf-metafields--menu-icon:after {
          content: none;
        }
        > .dropdown-menu {
          display: block;
          top: 100%;
          left: 0;
          background: $nav-dropdown-background;          
          z-index: -9999;             
          font-size: $nav-sub-font-size;     
          visibility: hidden;    
          background-size: cover;      
          @include translate(0, 10px);          
          @extend %opacity-zero;           
          @include transition(opacity 0.45s ease 0s);
          @media (max-width: 1199px) and (min-width: 992px) {
            width: 200px;
          }
          li {
            padding: 0 30px;
            @extend %flexbox;
            @extend %align-items-center;
            a {
              font-size: $nav-sub-font-size;  
              color: $nav-dropdown-link-color;
              padding: 8px 0;
              @extend %flexbox;
              span:first-child {
                color: inherit;
              }
              &:hover {
                color: $nav-dropdown-link-hover-color;
              }
            }
            &.dropdown-submenu {
              a {
                @extend %flex;
              }              
              .expand {
                width: 20px;
                text-align: end; 
                color: $nav-dropdown-link-color;               
                &:before {
                  @extend %before-expand;
                }                                
              }              
              .dropdown-menu {
                display: block;
                top: 0;
                left: 100%;                  
                z-index: -99;                  
                font-size: $nav-sub-font-size;     
                visibility: hidden;  
                @include translate(10px, 0);
                @extend %opacity-zero;
                @extend %transition-ease;                
              }
              &:hover {
                > .dropdown-menu {
                  visibility: visible;
                  z-index: 9999;
                  @extend %opacity;
                  @extend %translate-zero;
                }
              }
              &.flyout-left {
                .expand {
                  text-align: end;
                  @include rotate(180);                  
                }
                > a {
                  order: 2;
                  -webkit-order: 2;
                }
                .dropdown-menu {
                  left: auto;
                  right: 100%;
                  @include translate(-10px, 0);                  
                }
                &:hover {
                  > .dropdown-menu {
                    @extend %translate-zero;
                  }
                }
              }
            }
          }          
        }        
        &:hover,
        &.menu-showing--active {         
          > .dropdown-menu {
            visibility: visible;
            z-index: 9999;
            @extend %opacity;
            @extend %translate-zero;
          }
        }        
      } 
      &.active,     
      &:hover {       
        > a {
          color: $nav-link-hover-color;
        }        
      }
    }
  }
}
.ham-desk {
  .menu-on-hover {
    @include ab(100%, n, n, 0);
    background: $nav-dropdown-background;
    width: 242px;
    text-align: start;
    @extend %opacity-zero;
    z-index: 9999;
    visibility: hidden;
    @include translate(30px, 50px);
    @extend %transition-ease;
    @include box-shadow(3px 8px 9px rgba(#868484, 0.5));
    @media (min-width: 1200px) and (max-width: 1250px) {
      @include translate(0, 50px);
    }
    .menu-top {
      padding: 15px 0 0 25px;
      border-bottom: 1px solid $body-border-color;
      border-top: 2px solid $body-link-color;
      &:before {
        content: "";
        border: solid 10px transparent;
        @include ab(-20px, n, n, 30px);
        border-bottom-color: $body-link-color;
      }
      &:after {
        content: "";
        border-bottom: 9px solid #fff;
        border-left: 9px solid transparent; 
        border-right: 9px solid transparent;
        @include ab(-7px, n, n, 31px);
        z-index: 100;
      }
    }
    .menu-middle {
      @include padding-start(25px);
      border-bottom: 1px solid $body-border-color;
      .title {
        margin-bottom: 12px;
      }
    }
    .menu-bottom {
      @include padding-start(25px);
      .title {
        margin-bottom: 10px;
      }
      .list-inline {
        li {
          @include margin-end(15px);
        }
      }
    }
    ul {
      list-style: none;
      @include padding-start(0);
      li {
        margin-bottom: 13px;
        a {
          @extend %body-general-color;
          &:hover {
            color: $body-link-color;
          }
        }
        &.active {
          a {
            color: $body-link-color;
          }
        }
        &.li-lang {
          &.active {
            border: 1px solid $body-link-color;
          }
        }
        &:last-child { 
          margin-bottom: 20px;
        }
        &.li-lang:last-child {
          margin-bottom: 13px;
        }
      }
    }
    img {
      width: auto;
    }
    .title {
      font-weight: 600;
      text-transform: uppercase;  
      margin-top: 20px;
      display: block;
    }
  }
.menu-on-hover.opened {
    visibility: visible;
    opacity: 1;
    z-index: 9999;
    @include translate(30px, 30px);
    @media (min-width: 1200px) and (max-width: 1250px) {
      @include translate(0, 30px);
    }
  }
