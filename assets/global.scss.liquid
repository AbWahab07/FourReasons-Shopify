  &.style-9 {
    .header-desktop {
      .header-main .table-row {        
        .table-wrap-items {          
          > div {
            @extend %flex-100;
            max-width: 100%;
            text-align: center;
          }
          .header-logo {
            @include ab(30px, 0, n, 0);
            padding: 0 10px;
          }
          .header-icons {
            @include ab(n, 0, 30px, 0);
            @include transition(all 0.2s ease-out);
