      color: $header-overlay-icons-color-mob;
    }
    .header-mobile .header-main .table-row .header-mobile--right-content div .num-items-in-wishlist .number, 
    .header-mobile .header-main .table-row .header-mobile--right-content div .basket .number {
      background: $header-overlay-icons-number-bg-mob;
      color: $header-overlay-icons-number-color-mob;
      @if $header-overlay-icons-number-bg-mob == rgba(0, 0, 0, 0) {
        @extend %font-size-2;
      }
    }
  }
  .header-content {
    background: $header-mb-color;
    {% if settings.mobi_bg_image != blank %} 
    background-image: url({{ settings.mobi_bg_image | img_url: '992x' }});
    background-position: {{settings.desk_bg_position}};    
    background-size: {{settings.desk_bg_size}};    
    background-repeat: {{settings.desk_bg_repeat}}; 
    {% endif %}
  }  
}
.header-content {
  padding: 0;
  margin: 0;
  .header-desktop {
    .header-main {
      .table-row {    
        margin: 0 auto;           
        > div {            
          margin: 0 auto;
          position: relative;
        }
        .table-wrap-items {
          width: 100%;
          @extend %flexbox;          
          @extend %align-items-center;
          @extend %justify-content-between;         
          .horizontal-menu {
            @extend %flex;
          }
          .wrap-header-left {
            .search {
              width: 230px;
              max-width: 100%;
            }
            .topbar-popup .topbar-config,
            .topbar-popup .currencies_btn > a,
            .topbar-popup .currencies_btn > span {
              color: $header-icon-color;
              &:hover {
                color: $header-icon-hover-color;
              }
            }
          }
        }
      }
    }
  }
  .labels-wrapper {    
    text-transform: uppercase;    
    z-index: 2;
    .menu-label {
      display: block;
      font-size: 10px;
      font-weight: $body-weight;
      padding: 1px 3px 1px;
      @include margin-start(9px);
      position: relative;
      >span {
        &:after {
          content: "";            
          border-bottom: 4px solid transparent;
          border-top: 4px solid transparent;
          @include border-inline-start(4px solid transparent);
          @include ab(50%, -8px, n, n);
          @include translate(0, -50%);
        }
      }
      &.label-new {
        background: $label-new-bg;
        color: color-contrast($label-new-bg) !important;
        span {
          &:after {            
            @include border-inline-end(4px solid $label-new-bg);
          }
        }
      }
      &.label-hot {
        background: $label-hot-bg;
        color: color-contrast($label-hot-bg) !important;        
        span {
          &:after {
            @include border-inline-end(4px solid $label-hot-bg);
          }
        }
      }
      &.label-sal {
        background: $label-sale-bg;
        color: color-contrast($label-sale-bg) !important;
        span {
          &:after {
            @include border-inline-end(4px solid $label-sale-bg);
          }
        }
      }
    }
  }
  .header-main {
    position: relative;         
    .custom-btn-h1 {
      .btn {
        @include uppercase($navigation-uc);
        position: relative;        
        &.btn-custom-h1 {
          &:before {
            content: '';
            height: 2px;
            width: 25px;
            background: $nav-link-hover-color;
            @include ab(calc(50% - 1px), -14px, n, n);
          }    
        }                                    
      } 
    }
    .widget-social-icons {
      li {       
        a {
          color: $header-icon-color;
          background: $header-icons-bg;
          font-size: $header-icon-font-size;
          padding: $header-icon-padding;
          border-radius: $header-icon-rd;      
          margin: 0 12px;    
          &:hover {
            color: $header-icon-hover-color;
            background: $header-icons-hover-bg;
          }
        }
        &:not(:first-child) {          
          a {
            @include margin-start(0);
          }
        }
      }
    }
  }  
  &.style-1 {
    display: block;
    .header-desktop {
      .header-main {        
        .table-row {          
          .horizontal-menu {
            @extend %flex;            
            &:not(:first-child) {              
              @include padding-start(30px);
            }
            &:not(:last-child) {              
              @include padding-end(30px);
            }
          }
          .header-icons {
            @include margin-start(auto);
          }          
        }
      }
    }
  }
  &.style-2 {
   .header-desktop {
    .header-logo {
      padding: 0 20px;
    } 
    .header-icons {
      @extend %flex;
      ul {
        @extend %justify-content-flex-end;
      }
    }
   }   
  }
  &.style-3 {
    .header-desktop {
      .header-main {                
        .header-logo {
          padding: 0 30px;
        } 
        .wrap-header-left,
        .wrap-header-right {
          @extend %flex;          
        }   
        .wrap-header-left .horizontal-menu .menu-list {
          @extend %justify-content-flex-end;
        }        
        .wrap-header-right {
          @extend %align-items-center;
          @extend %justify-content-between;
        }
        .table-row {
          {%if settings.nav_bg_type == 'color' %}
          background: $bg-nav-color;
          {% else %}
          background-image: url({{ settings.nav_bg_image | img_url: '1920x' }});
          background-position: {{settings.nav_bg_position}};    
          background-size: {{settings.nav_bg_size}};    
          background-repeat: {{settings.nav_bg_repeat}}; 
          {% endif %}
        }
        .header-icons {
          ul li {           
            &.li-cart .cart-target {
              .cart-dd {
                top: 100%;
                &:before {
                  content: none;
                }
              }
            }
          }
        }
      }
    }
  }
  &.style-4 {
    .modal.show {      
      {%if settings.nav_bg_type == 'color' %}
      background: rgba($bg-nav-color, 0.85);
      {% else %}
      background-image: url({{ settings.nav_bg_image | img_url: '1920x' }});
      background-position: {{settings.nav_bg_position}};    
      background-size: {{settings.nav_bg_size}};    
      background-repeat: {{settings.nav_bg_repeat}}; 
      {% endif %}
      height: 100vh;
    }
    .modal-dialog {
      max-width: 100%;
      margin: 0;
      .modal-content {
        background: none;    
        height: 100vh;    
        .modal-header {            
          .close {                                      
            color: $nav-link-color; 
            @extend %opacity;
            @include fixed(40px, n, n, 50px);
            &:before {
              @extend %font-icon;
            }
            &:hover {
              @include opacity(0.7);
            }
          }
        }
        .modal-body {
          padding: 0;
          width: 100%;
          max-height: 100vh;
          @extend %flexbox;
          @extend %align-items-center;     
          @extend %justify-content-center;       
          .vertical-menu {   
            width: 100%;
            max-width: 1170px;
            padding: 0 15px;           
            .menu-list {                
              list-style: none;
              max-height: 90vh;
              overflow-y: auto;
              padding: 15px 0;
              > li { 
                text-align: center;                  
                &:not(:last-child) {
                  padding-bottom: 20px;
                }
                > a {
                  color: $nav-link-color;                  
                  font-size: $nav-font-size;
                  line-height: 1.2;
                  font-weight: $nav-weight;
                  @include uppercase($navigation-uc);
                  @extend %flexbox;
                  @extend %align-items-center;     
                  @extend %justify-content-center;
                  &:hover {
                    color: $nav-link-hover-color;
                  }
                }
                .dropdown-menu {                  
                  @extend %transition-ease;
                  position: static;
                  margin: 0 auto;
                  border: none;
                  text-align: center;
                  background: $nav-dropdown-background;
                  .back-prev-menu {
                    display: none;
                  }
                  li a {
                    font-weight: $nav-sub-weight;
                    font-size: $nav-sub-font-size;
                    color: $nav-dropdown-link-color;
                    padding: 5px 0;
                    display: inline-block;
                    &:hover {
                      color: $nav-dropdown-link-hover-color;
                    }
                  }
                }
                &.dropdown {
                  > a > .expand:before {
                    content: '\e905';
                    color: $nav-link-color;   
                    font-size: 12px;
                    padding-left: 10px;
                    @include opacity(0.7);
                    @extend %font-family;
                    @extend %transition-ease;
                  }
                  &:hover {                    
                    > a > .expand:before {
                      color: $nav-link-hover-color;
                    }
                    > .dropdown-menu {
                      display: block;
                      z-index: 9999;      
                      border: none;
                      text-align: center;     
                    }
                  }
                }
                &.mega-menu {
                  > .dropdown-menu .dropdown-menu {
                    display: block;
                  }
                }
              }
            }
          }
        }
      }
    }
    .header-desktop {
      .table-row {        
        .table-wrap-items {
          @extend %justify-content-flex-end;
          > div {
            &:not(:first-child) {
              @include padding-start(17px);
            }
          }
        }        
        .header-logo {
          @include margin-end(auto);
        }
      }      
    }    
  }
  &.style-5,
  &.style-6,
  &.style-7,
  &.style-8 {   
    .header-desktop .header-main .table-row[data-index="1"] {
      {%if settings.nav_bg_type == 'color' %}
      background: $bg-nav-color;
      {% else %}
      background-image: url({{ settings.nav_bg_image | img_url: '1920x'}});
      background-position: {{settings.nav_bg_position}};    
      background-size: {{settings.nav_bg_size}};    
      background-repeat: {{settings.nav_bg_repeat}}; 
      {% endif %}
    }
  }
  &.style-5,
  &.style-6 {
    .header-desktop .header-main .table-row {      
      .wrap-header-left,
      .wrap-header-right {
        @extend %flex;        
      }
      .wrap-header-right {
        @extend %flexbox;
        @extend %justify-content-flex-end;
        .header-icons {
          .list-inline {
            @extend %justify-content-flex-end;
          }
        }
      }
      .wrap-header-center {
        padding: 0 20px;
      }
    }
  }
  &.style-5 {
    .header-desktop {
      .header-main {        
        .table-row {    
          .wrap-header-left {
            @extend %flexbox;
            @extend %flex-wrap;
            @extend %align-items-center;
            > div:not(:first-child) {
              @include padding-start(10px);
            }
          }                                    
        }  
        .table-row[data-index="1"] {
          .group-custom-text {
            .custom--text {
              p {
                color: $nav-link-color;
              }
            }
          }
          .searchbox .search {            
            input {
              border-color: rgba($nav-link-color, 0.3);                
              color: $nav-link-color;
              @include opacity(0.8);
              @include placeholder {
                color: $nav-link-color;
              }
            }
            .search-icon {
              color: $nav-link-color;
              @include opacity(0.8);
              &:hover {
                color: $nav-link-hover-color;
              }
            }
          }
        }              
      }
    }    
  }  
  &.style-6 {
    .header-desktop {
      .header-main {          
        .wrap-header-left {
          > div:not(:first-child) {
            @include padding-start(15px);
          }
          .custom--text p {
            @extend %font-size-1;
          }
        }
        .wrap-header-right {
          > div:not(:last-child) {
            @include padding-end(15px);
          }
          .group-custom-text {
            margin-right: -15px;
            margin-right: -15px;
            > div:not(:last-child) {
              @include padding-end(20px);
            }
          }
        }
        .table-row[data-index="1"] {
          .horizontal-menu + .header-icons > ul {
            color: $nav-link-color;  
            > li > div > a,
            > li .cart-target > a {
              color: $nav-link-color;  
              &:hover {
                @media (min-width: 1025px) {
                  color: $nav-link-hover-color;
                }
              }              
            }
          }
        }
      }
    }    
  }
  &.style-7,
  &.style-8 {
    .header-desktop {
      .wrap-header-center .searchbox,
      .horizontal-menu + .searchbox {
        width: 540px;        
        border: 1px solid rgba($header-icon-color, 0.2);
        @extend %flexbox;
        @extend %align-items-center;
        > select {
          @include flex-box(142px);          
          border: none;
          @include border-inline-end(1px solid rgba($header-icon-color, 0.1));
          color: rgba($header-icon-color, 0.6);
          padding: 0 25px 0 14px;
          line-height: 40px;
          height: 40px;
          background-image: url({{"download.png" | asset_url}});
          background-repeat: no-repeat;
          background-size: 10px;
          background-position: calc(100% - 12px) 50%;
        }
        .search {
          @extend %flex;
          input {
            height: 40px;    
            border: none;  
          }
          .search-icon {
            height: 42px; 
            width: 42px;                                     
            background: $main-color;
            color: $main-color-text;
            @include ab(-1px, n, n, -1px);
          }
        }    
      }  
      .horizontal-menu + .searchbox {
        width: 25%;
      }
      .group-custom-text {
        @extend %flexbox;
        > div {
          @extend %flex;
          padding: 0 20px;
        }
        .custom--text {
          @extend %flexbox;
          @extend %align-items-center;
          // @extend %justify-content-center;
          height: 100%;
          p {
            i, span {
              @extend %flexbox;            
            }
          } 
        }
      }   
    }
  }
  &.style-7 {
    .header-desktop {
      .header-main {
        .table-row {                      
          .header-icons li > div > a {
            text-transform: uppercase;
          }
        }
        .table-row[data-index="1"] {         
          .searchbox.form-rd {
            .form-control {
              background: $nav-link-color;
              border-color: $nav-link-color;
              color: color-contrast($nav-link-color);    
              // @include opacity(0.6);
              @include placeholder {
                color: color-contrast($nav-link-color);
              }
            }
            .search-icon {
              color: color-contrast($nav-link-color);   
              // @include opacity(0.6);                    
            }
          }
        }
      }      
      .group-custom-text {
        .has-border {
          @include border-inline-start(1px solid rgba($header-icon-color, 0.1));  
        }
        .custom--text {             
          .d-icon {
            line-height: 100%;
            i {
              font-size: 21px;
              @include margin-end(25px);
            }
          }       
          p, a, .d-inline-block > span {
            @extend %justify-content-center;
            strong {
              font-weight: 700;
              @extend %font-size-4;
            }
            span {
              @extend %flex-100;
              @extend %justify-content-center;
              @extend %font-size-2;
              text-transform: uppercase;
            }
            i {
              @extend %main-color;    
              @include margin-end(6px);          
            }
          }
        }
        .d-inline-block {
          text-align: center;          
          flex-basis: auto;
          > span {
            display: block;
            &:first-of-type {
              font-weight: 700;
              @extend %font-size-4;
            }
            &:last-of-type {
              @extend %flex-100;
              @extend %justify-content-center;
              @extend %font-size-2;
              text-transform: uppercase;
            }            
          }  
          &.d-icon {
            + .d-inline-block {
              text-align: start;
            }
          }                
        }
      }
    }
  }
  &.style-8 {    
    .header-desktop {      
      .header-main {          
        .vertical-menu {
          @include margin-end(10px);
        }
        .vertical-menu,
        .wrap-header-left {
          @include flex-box(270px);          
        }
        .wrap-header-center {
          @include margin-end(auto);
          .group-custom-text {
            margin-bottom: 20px;
          }
          &.search-center {
            @include margin-start(auto);
          }
        }
        .horizontal-menu {
          @extend %flex;
        }
      }      
    }  
  }  
  &.style-9, &.style-10 {    
    .header-desktop {
      aside.header-main,
      aside.header-fixed-column {          
        height: 100%;
        width: 90px;        
        z-index: 998;    
        background: $header-bg;
        @include box-shadow(0 0 30px rgba($dark-color, 0.05));
        @include fixed(0, 0, n, n);
      }
      .header-main .table-row,
      aside.header-fixed-column {
        height: 100%;
        padding: 0;
        .table-wrap-items {
          height: 100%;
          @extend %flex-100;
          .navbar-vertical-collapse {            
            .responsive-menu {
              height: 30px;
              width: 30px;                                               
              margin: 0 auto;
              span {                
                display: block;                
                width: 100%;        
                text-align: center;                                        
                i {                  
                  font-size: 30px;
                  text-align: center;
                  color: $header-icon-color;
                  &:before {
                    @extend %transition-ease;
                  }
                }
              }
            } 
          }
        }   
        .vertical-menu {
          @include ab(0, 100%, n, n);        
          @include transition(all 0.2s ease-out);
          @include translate(-40px, 0);
          @extend %opacity-zero;
          visibility: hidden;
          width: 320px;        
          height: 100vh;
          z-index: 9999;
          padding: 0;
          .main-menu {
            @extend %flexbox;
            @extend %align-items-center;          
            width: 100%;
            height: 100%;
            background: rgba($header-bg, ($header-bg-transparent / 100));
            .menu-list {
              width: 100%;
              max-height: 100vh;
              overflow-y: auto;             
              .dropdown {
                position: static;
                .expand {
                  width: 25px;
                  text-align: center;
                  line-height: 14px;
                  &:before {
                    @extend %before-expand;
                  }
                }
                .dropdown-menu {
                  height: 100%;                  
                  z-index: -999;                  
                  visibility: hidden;
                  background: $nav-dropdown-background;
                  border-left: 1px solid $body-border-color;
                  border-right: 1px solid $body-border-color;
                  align-content: center;
                  -webkit-align-content: center;
                  @include ab(0, 100%, n, n);        
                  @include flexbox;
                  @extend %flex-wrap;
                  @extend %align-items-center;
                  @include transition(all 0.2s ease-out);                  
                  @extend %opacity-zero;                  
                  @include translate(10px, 0);
                  &:after {
                    content: '';
                    @include ab(0, -5px, 0, n);
                    width: 10px;
                  }
                  .back-prev-menu {
                    display: none;
                  }
                  li {
                    padding: 0;
                    @extend %flex-100;  
                    a {
                      color: $nav-dropdown-link-color;
                      padding: 8px 30px;
                      display: block;                      
                      span {
                        color: inherit;
                      }
                      &:hover {
                        color: $nav-dropdown-link-hover-color;
                      }
                    }                    
                  }
                }                
                &.dropdown-submenu {
                  .expand {
                    color: $nav-dropdown-link-color;
                  }                 
                }
                &:hover {
                  > .dropdown-menu {
                    @extend %translate-zero;
                    @extend %opacity;      
                    z-index: 9999;    
                    visibility: visible;                              
                  }
                }
              }
              li {
                padding: 0 10px;
                @extend %flexbox;
                @extend %align-items-center;                              
                a {
                  @extend %flexbox;
                  @extend %flex-wrap;       
                  @extend %justify-content-center;
                  @extend %flex;
                }
              }
              > li {    
                @extend %justify-content-center;            
                > a {                             
                  color: $nav-link-color;                
                  line-height: 1.4;                
                  padding: 13px 0;
                  font-weight: $nav-weight;
                  font-size: $nav-font-size;
                  @include uppercase($navigation-uc);
                  @include flex(inherit);
                  &:hover {
                    color: $nav-link-hover-color;
                  }
                  > .expand {
                    color: inherit;   
                  }
                }
                > .expand {
                  color: $nav-link-color;   
                }
                &.mega-menu {
                  @extend %menu-banner;   
                  > .dropdown-menu {
                    width: calc(100vw - 440px) !important;
                    max-width: 1000px;
                    padding: 32px 18px 0;
                    max-height: 100vh;
                    overflow: hidden;
                    overflow-y: auto;
                    > .row {
                      margin: 0;
                      width: 100%;
                    }
                    .expand {
                      display: none;
                    }
                    .mega-sub-link,
                    .mega-block-items-inner {                      
                      > a {
                        display: block;                        
                        color: $nav-dropdown-link-color;                                             
                        &:hover {
                          color: $nav-dropdown-link-hover-color;
                        }
                      }  
                      &.no-title-0,
                      &.no-title-1,
                      &.no-title-2 {
                        > a, > span {
                          display: none;
                        }
                      }
                      &.mega-mutiple-link {
                        @extend %flex-wrap;
                        padding: 0;
                        > a {
                          padding: 0;
                        }
                        > div {
                          @include flex-box(100%);                          
                        }                        
                        &:not(:last-child) {
                          margin-bottom: 22px;
                        }
                      }
                    }
                    .dropdown-menu {
                      position: static;
                      display: none;
                      width: 100%;
                      border: none;
                      padding: 0;
                      margin: 0 0 22px;
                      visibility: visible;  
                      z-index: 1;
                      background: none;
                      @extend %translate-zero;
                      @extend %opacity;                            
                    }
                    &.dropdown-menu-tabs {
                      .mega-navigation-with-tab {
                        @extend %align-items-center;
                      }
                      .mm-tab-col-title {                            
                        ul {
                          margin: 0;
                          padding: 0;
                          width: 100%;
                          li {
                            cursor: pointer;
                            padding: 10px 0;
                            text-transform: uppercase;                
                            font-weight: $nav-sub-weight;
                            @extend %body-font-size;
                            color: $nav-dropdown-link-color;
                            border-bottom: 1px solid $body-border-color;
                            position: relative;           
                            &:first-child {
                              border-top: none;
                            }
                            &.active {              
                              color: $nav-dropdown-link-hover-color;  
                            }
                          }
                        }
                      }
                      .mm-tab-col-content {
                        position: relative;
                        .tab-content-inner {
                          @include transition(all 0.75s ease-in-out 0s);
                          @include ab(0, 15px, n, 15px);
                          @extend %opacity-zero;
                          visibility: hidden;
                          z-index: -99;                
                        }                        
                      }
                    }
                  }
                  &:hover {
                    > .dropdown-menu {
                      .dropdown-menu {
                        display: block;
                      }
                      .mm-tab-col-content .tab-content-inner.active {
                        position: static;
                        visibility: visible;
                        z-index: 1;
                        @extend %opacity;
                      }
                    }
                  }
                }
              }
            }
          }
        }   
      }      
      &.show-menu {        
        .header-main .table-row,
        aside.header-fixed-column {
          .table-wrap-items .navbar-vertical-collapse .responsive-menu span i:before {
            content: '\e81f';
            font-size: 20px;
          }
          .vertical-menu {          
            @extend %translate-zero;
            @extend %opacity;          
            visibility: visible;          
            border-left: 1px solid $body-border-color;
            border-right: 1px solid $body-border-color;
            background: $bg-nav-color;
          }
        }        
      }
    }
