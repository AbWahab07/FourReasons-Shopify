      }            
    }
    .post-body {
      padding: 15px 0 0;
      .post-title {
        margin: 0;
        a {
          display: block !important;
          padding: 0 !important;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
          color: $nav-dropdown-link-color;
          font-size: $nav-sub-font-size;
          font-weight: $nav-sub-weight;
          @include uppercase($navigation-uc);                
          &:hover {
            color: $nav-dropdown-link-hover-color;
          }
        }
      }
    }
  }
}
.horizontal-menu,
.vertical-menu {
  .menu-list, .dropdown .menu-list {
    .dropdown-menu {
      font-family: $font-standard !important;
    }
    > li.dropdown.mega-menu {
      @extend %menu-banner; 
      .listing-item {
        &:not(:last-child) {
          margin-bottom: 20px;
        }
        .product-image {          
          @include padding-end(5px);
          > a {
            @extend %padding-product-list;
            position: relative;
            display: block;
            img, .empty-image {
              @extend %product-image;
            }
            .empty-image {
              @include ab(0, 0, n, n);
              width: 100%;
              height: 100%;
            }
          }
        }        
        .product-content {          
          @include padding-start(10px);
          .product-name {
            margin-bottom: 3px;
            > a {
              color: $nav-dropdown-link-color;
              font-size: $nav-sub-font-size;
            }
          }
        }
      }   
      .mega-overlay-content,
      .mega-menu--block_image-item {
        position: relative;
        > a {
          display: block !important;
          padding: 0 !important;
        }
      }
      .mega-image-width--fill:not(.mega-overlay-content) {
        @extend %transition-ease;
        @media (min-width: 1025px) {
          &:hover {
            @include opacity(0.5);
          }
        }
      }
      .mega-overlay-content.mega-image-width--fill {
        position: relative;
        overflow: hidden;
        padding: 0;
        img {          
          height: 100%;
          @include transition(all 0.75s ease-out 0s);
        }
        &:before {
          @include before;
        }
        &:hover {
          &:before {
            @include before-hover;
          }
          img {
            @include scale(1.15, 1.15);
          }
        }
      }
      .mega-overlay-content--item {
        position: absolute;  
        pointer-events: none;
      }        
      .mega-image-width--default {
        a > span {         
          text-align: center;         
        }
      }  
      .mega-block-image--sub-text > p > a {
        display: inline-block;
        text-decoration: underline;
      }
      .mega-type--image .mega-block-content-inner > a {
        display: block;
      }      
      .mega-block-items:not(.mega-type--menu) {
        .mega-block-items-inner > a,
        > a {
          margin-left: 0 !important;
          margin-right: 0 !important;
          display: block;
        } 
        .dropdown-menu li {
          padding: 0; 
        }  
      }
      > .dropdown-menu {
        padding: 32px 3px 0;            
        .mega-sub-link,
        .mega-block-items-inner {
          > a {                                   
            color: $nav-dropdown-link-color;                         
            &:hover {
              color: $nav-dropdown-link-hover-color;
            }
          }
          > .expand {
            display: none;
          }
          &.no-title-0,
          &.no-title-2,
          &.no-title-1 {
            > a {
              padding: 0;
              margin: 0 !important;
            }
          }
          &.mega-mutiple-link {
            @extend %flex-wrap;
            padding: 0;
            > a {
              padding: 0;
            }
            > div {
              @include flex-box(100%);
            }               
            &:not(:last-child) {
              margin-bottom: 22px;
            }
          }
        }
        .mega-block-items-inner > a {
          display: block;         
        }
      }
    }
  }
}
.horizontal-menu {
  .menu-list > li.dropdown.mega-menu {
    position: static;                           
    > .dropdown-menu { 
        margin: 0 15px;  
        &:after {
            content: '';
            height: 10px;
            @include ab(-8px, 0, n, 0);
        }                                 
        &.position-right {              
            right: 0;              
            @include ab(100%, auto, n, 0);
        }            
        &.position-center {              
            @include ab(100%, 50%, n, n);
            @include translate(-50%, 10px);
        }   
        &.column-4 {
            width: 100%;
            max-width: calc(100% - 30px);
        }
        &.column-3 {
            width: 75%;
        }
        &.column-2 {
            width: 50%;
        }
        &.column-1 {
            width: 25%;
        }
        .dropdown-menu {
            display: none;
            position: static;
            border: none;
            width: 100%;
            background: transparent;
            padding: 0;
            margin: 0 0 22px;                                                        
        }            
    }  
    .dropdown-menu-tabs {
      padding: 0;            
      .mm-tab-col-title {       
        padding: 0;        
        @include padding-start(15px);
        ul {
          margin: 0;
          padding: 0;
          width: 100%;
          li {
            cursor: pointer;
            padding: 12px 0;
            text-transform: uppercase;     
            font-weight: $nav-sub-weight;           
            @extend %body-font-size;
            color: $nav-dropdown-link-color;            
            border-bottom: 1px solid $body-border-color;
            position: relative;           
            &:first-child {
              border-top: none;
            }
            &.active {              
              color: $nav-dropdown-link-hover-color;  
            }
          }
        }
      }
      .mm-tab-col-content {
        position: relative;
        .tab-content-inner {
          @include transition(all 0.75s ease-in-out 0s);
          @include ab(0, 0, n, n);
          @extend %opacity-zero;
          @extend %flexbox;
          @extend %flex-direction-column;
          height: 100%;
          width: 100%;
          visibility: hidden;
          z-index: -99;     
          padding: 32px 3px 0;
        }              
      }
    }
    &:hover,
    &.menu-showing--active {           
      > .dropdown-menu {
          @extend %translate-zero;
          &.position-center {
            @include translate(-50%, 0);
          } 
          .dropdown-menu {
            display: block;
          }
      }
      .dropdown-menu-tabs .mm-tab-col-content .tab-content-inner.active {
        position: static;
        visibility: visible;
        z-index: 1;
        @extend %opacity;        
      }   
    }
  }
}
.vertical-menu .dropdown .menu-list li.dropdown.mega-menu {            
  > .dropdown-menu {    
    .dropdown-menu {
      position: static;
      border: none;
      visibility: visible;
      width: 100%;
      background: transparent;
      padding: 0;
      margin: 0 0 22px;
      display: none;
      @extend %opacity;
      @extend %translate-zero;              
      li {
        padding: 0;
      }      
    }
  }
  &.menu-showing--active,
  &:hover {
    > .dropdown-menu .dropdown-menu {
      display: block;
    }
  }
}

