div[class*='carousel'][data-owl-xs]:not(.owl-loaded){
  display: flex!important;
  overflow: hidden;
  > .products-grid-item .product-card, .category-item, .logo-list-item, .spc-block, .banner-item,
  .customer-item-inner, .plan-item {
    margin-bottom: 0;      
  }
  .post-item, .banner-item, > .products-grid-item {
    padding: 0;
  }
  .product-card {
    width: 100%;    
    margin: 0;     
  }
  @for $i from 1 to 8{
    $pe: 100/$i+'%';

    @if $i < 6{
      &[data-owl-xs="#{$i}"] > * {flex: 0 0 #{$pe};}
      @media (min-width: 576px) {&[data-owl-sm="#{$i}"] > * {flex: 0 0 #{$pe};}}
    }
    @media (min-width: 768px) {&[data-owl-md="#{$i}"] > * {flex: 0 0 #{$pe};}}
    @media (min-width: 992px) {&[data-owl-lg="#{$i}"] > * {flex: 0 0 #{$pe};}}
    @media (min-width: 1200px){&[data-owl-xl="#{$i}"] > * {flex: 0 0 #{$pe};}}
    @if $i < 8{
      $pa: $i*5;
      &[data-padding-xs="#{$pa}"] > * {@include padding-end(#{$pa+'px'});}
      @media (min-width: 768px){&[data-padding-md="#{$pa}"] > * {@include padding-end(#{$pa+'px'});}}
    }
    @if $i < 5{
      @for $j from 1 to 9{
        $s: $i + $j/10;
        $npe: 100/$s + '%';
        &[data-owl-xs*="#{$i}.#{$j}"] > * {flex: 0 0 #{$npe};}
        @media (min-width: 576px) {&[data-owl-sm*="#{$i}.#{$j}"] > * {flex: 0 0 #{$npe};}}
        @media (min-width: 768px) {&[data-owl-md*="#{$i}.#{$j}"] > * {flex: 0 0 #{$npe};}}
        @media (min-width: 992px) {&[data-owl-lg*="#{$i}.#{$j}"] > * {flex: 0 0 #{$npe};}}
        @media (min-width: 1200px){&[data-owl-xl*="#{$i}.#{$j}"] > * {flex: 0 0 #{$npe};}}
      }
    }
  }  
}
