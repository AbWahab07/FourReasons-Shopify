%close-btn {
  width: 44px;
  height: 44px;
  margin: 0;
  padding: 0;
  background: none;
  border: none;
  color: $body-title-color;
  z-index: 9;
  cursor: pointer;
  @include opacity(0.65);
  @include ab(0, n, n, 0);
  svg, i, span {
    display: none;
  }
  &:before {
    content: "\e90a";
    font-family: 'arenafont';   
    display: block; 
    color: inherit;
    width: 100%;
    height: 100%;
    line-height: 44px;
    font-weight: 400;
    text-align: center;
    font-size: $body-font-size + 2;    
    z-index: 1;    
  }
  &:hover {
    @include opacity(1);
    color: $main-color;
  }
}
