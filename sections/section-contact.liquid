{%- if section.settings.boxed_layout -%}{%- assign _layout = 'boxed' -%}{%- else -%}{%- assign _layout = 'full' -%}{%- endif -%}
{%- assign _s_type    				= 'contact-page' -%}
{%- assign _lazyload  				= settings.use_lazyload -%}
{%- assign _padding   				= section.settings.section_padding -%}
{%- assign _bg_color  				= section.settings.section_bg_color -%}
{%- assign _bg_image  				= section.settings.section_bg_image -%}
{%- assign _ib_pos	  				= section.settings.ib_position -%}
{%- assign _ib_align	  			= section.settings.ib_content_align -%}
{%- assign _ib_style	  			= section.settings.ib_style -%}
{%- assign _ib_heading 				= section.settings.ib_heading -%}
{%- assign _ib_subheading 			= section.settings.ib_subheading -%}
{%- assign _ib_heading_size 		= section.settings.ib_heading_size -%}
{%- assign _ib_heading_align 		= section.settings.ib_heading_content_align -%}
{%- assign _block_align 			= section.settings.block_align -%}
{%- assign _block_spc 				= section.settings.spacing_items -%}

{%- if section.settings.sec_head_break_container -%}{%- assign sec_headClass = ' f-break' -%}{%- endif -%}
{%- capture _bg_image -%}{% if _bg_image != blank %}{{_bg_image | img_url: '1920x'}}{% endif %}{%- endcapture -%}
{%- capture image_empty -%}<div class="empty-image">{{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}</div>{%- endcapture -%}

{%- include 'include-item-per-row' -%}
{%- render 'theme-snippet' ,type: 'section_id' -%}

<div class="home-section-contact layout-{{_layout }} section-show-{{section.id}}{% if _bg_image != blank %} bg-image{% if _lazyload %} lazyload" data-bgset="{{_bg_image}} 1920w{%- endif -%}{%- endif -%}" data-section-type="{{_s_type}}" data-section-id="{{section.id}}">
  {%- if _layout == 'boxed' -%}<div class="container">{%- endif -%}

    {%- include 'head-title',extended_class: sec_headClass -%}

	{%- capture _ib_content -%}
		{%- unless _ib_heading == blank and _ib_subheading == blank -%}
			<div class="ib-heading text-{{_ib_heading_align}}">
				{%- unless _ib_heading == blank -%}
				<div class="ib-heading h{{_ib_heading_size}}">{{_ib_heading}}</div>
				{%- endunless -%}
				{%- unless _ib_subheading == blank -%}
				<div class="ib-subheading">{{_ib_subheading}}</div>
				{%- endunless -%}
			</div>
		{%- endunless -%}
		<div class="ib-content row">
		{%- for block in section.blocks -%}
			{%- if block.type == 'iconbox' -%}

				{%- assign _ibfont_icon 	= block.settings.font_icon -%}
				{%- assign _ibimage 		= block.settings.image -%}
				{%- assign _ib_title 		= block.settings.ib_title -%}
				{%- assign _ib_content 		= block.settings.ib_content -%}
				{%- assign _ib_show_social 	= block.settings.show_social -%}

				{%- capture _ib_textContent -%}{{_ib_title}}{{_ib_content}}{%- endcapture -%}

				{%- capture _ib_Icon -%}
					{%- unless _ibimage == blank -%}
						<span class="ib-item-contain ib-icon-image">
							{%- include 'image-style', image: _ibimage, image_width: '[100,200]' -%}
						</span>
					{%- else -%}
						<span class="ib-item-contain ib-icon">
							{%- include 'icons', type: 'icon_free', name: _ibfont_icon -%}
						</span>
					{%- endunless -%}
				{%- endcapture -%}

				{%- unless _ibfont_icon == blank and _ibimage == blank and _ib_textContent == blank -%}
					<div class="ib-style-{{_ib_style}} {{items_class}}"{{ block.shopify_attributes }}>
						<div class="ib-item text-{{_ib_align}}">
							<div class="ib-item-inner">
								{%- unless _ib_textContent contains '[icon]' -%}
									<div class="ib-header">{{_ib_Icon}}</div>
								{%- endunless -%}
								<div class="ib-content">
									{%- if _ib_style == '3' -%}
										{%- unless _ib_title == blank 	-%}<span class="ib-title fw-{{section.settings.ib_title_fw}}">{{_ib_title | replace: '[icon]',_ib_Icon }}</span>{%- endunless -%}
										{%- if _ib_show_social 			-%}<span class="ib-content">{%- include 'social-media-icons' -%}</span>
										{%- elsif _ib_content != blank 	-%}<span class="ib-content">{{_ib_content | replace: '[icon]',_ib_Icon }}</span>
										{%- endif -%}
									{%- else -%}
										{%- unless _ib_title == blank 	-%}<div class="ib-title fw-{{section.settings.ib_title_fw}}">{{_ib_title | replace: '[icon]',_ib_Icon }}</div>{%- endunless -%}
										{%- if _ib_show_social 			-%}<div class="ib-content">{%- include 'social-media-icons' -%}</div>
										{%- elsif _ib_content != blank 	-%}<div class="ib-content">{{_ib_content | replace: '[icon]',_ib_Icon }}</div>
										{%- endif -%}
									{%- endif -%}
								</div>
							</div>
						</div>
					</div>
				{%- endunless -%}

			{%- endif -%}
		{%- endfor -%}
		</div>
	{%- endcapture -%}

    <div class="contact-content">
	    <div class="row align-items-md-{{_block_align}}{% unless _block_spc %} no-gutters{% endunless %}">
	      {%- for block in section.blocks -%}
	        {%- assign b_padding	= block.settings.block_padding -%}
	        {%- assign _width 		= block.settings.width -%}
	        {%- capture width -%}{%- include 'set-width-bootstrap',in_type: 'width', number: _width -%}{{_out}}{%- endcapture -%}

	        {%- case block.type -%}
	          	{%- when 'text_item' -%}{% continue %}
	          	{%- when 'iconbox' -%}
			        {%- if _ib_pos == 'block' -%}
			          	{%- if has_ib -%}{% continue %}{%- endif -%}
			          	{%- capture width -%}{%- include 'set-width-bootstrap',in_type: 'width', number: section.settings.ib_width -%}{{_out}}{%- endcapture -%}
			          	{%- assign has_show	  	= section.settings.show_shadow -%}
			        {%- else -%}
			        	{%- continue -%}
			        {%- endif -%}
	          	{%- when 'contact' -%}
		          	{%- assign has_show = block.settings.show_shadow -%}
		        {%- else -%}
		        	{%- assign has_show = false -%}
	        {%- endcase -%}
			{%- capture ext_class -%}{% if has_show %} has-shadow{% endif %} block-{{block.id}}{%- endcapture -%}

			<div class="contact-type-{{block.type}} {{width}}{{ext_class}}"{{ block.shopify_attributes }}>
				{%- capture contactTypeID -%}.section-show-{{section.id}} .contact-type-{{block.type}}.block-{{block.id}}{%- endcapture -%}
		        {%- case block.type -%}

		          	{%- when 'map' -%}
			          	{%- assign _min_height 	= block.settings.min_height -%}
			          	{%- assign _pin_img 	= block.settings.pin_image -%}
			          	{%- assign _api_key 	= block.settings.api_key -%}

			          	{%- unless _pin_img == blank -%}
			          		{%- assign _pin = _pin_img | img_url: '30x' -%}
			          	{%- else -%}
			          		{%- assign _pin = 'pin.png' | asset_url -%}
			          	{%- endunless -%}

			          	<div class="map-wrapper map-element-ui"
			          			data-latlong="{{ block.settings.contact_latlong | strip }}"
			          			data-zoom="{{ block.settings.contact_zoom_lv }}"
			          			data-scroll="{{ block.settings.contact_scroll }}"
			          			data-icon="{{ _pin }}"
			          			data-content="{{ shop.address.street }}, {{ shop.address.city }}, {{ shop.address.country }}">
			          		<div id="map_{{block.id}}" class="map map-element"></div>
			          	</div>

			          	{%- capture map_css -%}
			          		{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .map', css_value: _min_height, css_property: 'min-height' -%}
			          		{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' > div', css_value: b_padding -%}
			          		{{- contactTypeID}} .map:empty {background: center / cover no-repeat url({{'map-placeholder.jpg' | asset_url }});}
			          	{%- endcapture -%}
						{%- assign block_styles = block_styles | append: map_css -%}

		          	{%- when 'contact' -%}
						{%- assign _heading_align 		= block.settings.heading_content_align -%}
						{%- assign _heading_text 		= block.settings.heading_text -%}
						{%- assign _subheading_text 	= block.settings.subheading_text -%}
						{%- assign _fheading_size 		= block.settings.heading_size -%}
						{%- assign _fheading_color 		= block.settings.heading_color -%}
						{%- assign _fsubheading_color 	= block.settings.subheading_color -%}
						{%- assign _fbg_color 			= block.settings.bg_color -%}
						{%- assign _form_color 			= block.settings.form_color -%}
						{%- assign _form_border 		= block.settings.form_border -%}
						{%- assign _form_bg_color 		= block.settings.form_bg_color -%}
						{%- assign _btn_frm_align 		= block.settings.btn_frm_align -%}
						{%- assign _frm_btn_style 		= block.settings.frm_btn_inherit -%}
						{%- assign frm_btn_label 		= block.settings.btn_label -%}
						{%- assign _block_bg_img 		= block.settings.block_bg_img -%}
						{%- capture block_bg_img -%}{%- unless _block_bg_img == blank -%}{{_block_bg_img | img_url: '1920x'}}{%- endunless -%}{%- endcapture -%}

						<div class="contact-form-inner{% if _block_bg_img != blank and _lazyload %} lazyload" data-bgset="{{block_bg_img}} 1920w{% endif %}">
							{%- if _ib_pos == 'form' -%}{{_ib_content}}{%- endif -%}

							{%- unless _heading_text == blank and _subheading_text == blank -%}
								<div class="form-heading-content text-{{_heading_align}}">
									{%- unless _heading_text == blank -%}
										<div class="heading h{{_fheading_size}}" style="color:{{_fheading_color}};">{{_heading_text}}</div>
										{%- capture form_styles -%}
											{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner .form-heading-content .heading', css_value: block.settings.heading_margin, css_property: 'margin' -%}
										{%- endcapture -%}
										{%- assign block_styles = block_styles | append: form_styles -%}
									{%- endunless -%}

									{%- unless _subheading_text == blank -%}
										<div class="subheading" style="color:{{_fsubheading_color}};">{{_subheading_text}}</div>
										{%- capture form_styles -%}
											{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner .form-heading-content .subheading', css_value: block.settings.subhead_margin, css_property: 'margin' -%}
										{%- endcapture -%}
										{%- assign block_styles = block_styles | append: form_styles -%}
									{%- endunless -%}
								</div>
							{%- endunless -%}
			          		{%- include 'include-form',f_align :block.settings.text_align, f_btn_align: _btn_frm_align, frm_btn_style :_frm_btn_style -%}
						</div>

						{%- capture form_styles -%}
							{%- unless _block_bg_img == blank -%}
							{{- contactTypeID}} .contact-form-inner{
								background-position: {{block.settings.block_bg_position}};
								background-repeat: {{block.settings.block_bg_repeat}};
								background-size: {{block.settings.block_bg_size}}};
							{% unless _lazyload %}background-image: url({{block_bg_img}});{% endunless %}
							{%- endunless -%}
							{{- contactTypeID}} .contact-form-inner{background-color: {{_fbg_color}};}
							{{- contactTypeID}} input, {{contactTypeID}} textarea{background: {{_form_bg_color}};color:{{_form_color}};border: 1px solid {{_form_border}};}
							{{- contactTypeID}} {text-align: {{_btn_frm_align}};}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner', css_value: b_padding -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner input[type="text"]' 	,css_value: block.settings.form_height 	,css_property: 'height' -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner input[type="email"]' 	,css_value: block.settings.form_height 	,css_property: 'height' -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner input' 				,css_value: block.settings.form_rd 		,css_property: 'border-radius' -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .contact-form-inner textarea' 				,css_value: block.settings.form_rd 		,css_property: 'border-radius' -%}
						{%- endcapture -%}
						{%- assign block_styles = block_styles | append: form_styles -%}

			        {%- when 'text' -%}
				        {%- assign _heading_align 		= block.settings.heading_content_align -%}
				        {%- assign _heading_text 		= block.settings.heading_text -%}
				        {%- assign _heading_fw	 		= block.settings.heading_fw -%}
				        {%- assign _subheading_text 	= block.settings.subheading_text -%}
				        {%- assign _heading_size 		= block.settings.heading_size -%}
				        {%- assign _heading_color 		= block.settings.heading_color -%}
				        {%- assign _subheading_color 	= block.settings.subheading_color -%}
				        {%- assign _b_bg_color 			= block.settings.bg_color -%}
				        {%- assign _t_content_align		= block.settings.content_align -%}
				        {%- assign _t_heading_color		= block.settings.item_heading_color -%}
				        {%- assign _t_content_color		= block.settings.item_content_color -%}
				        {%- assign _t_heading_lheight	= block.settings.heading_lheight -%}

				        <div class="textbox-content-inner">
							{%- unless _heading_text == blank and _subheading_text == blank -%}
								<div class="textbox-heading-content text-{{_heading_align}}">
									{%- unless _heading_text == blank -%}
										<div class="heading h{{_heading_size}} fw-{{_heading_fw}}" style="color:{{_heading_color}};">{{_heading_text}}</div>
									{%- endunless -%}
									{%- unless _subheading_text == blank -%}
										<div class="subheading" style="color:{{_subheading_color}};">{{_subheading_text}}</div>
									{%- endunless -%}
								</div>
							{%- endunless -%}

							{%- if section.blocks[forloop.index].type == 'text_item' -%}
								<div class="textbox-items row">
									{%- for block in section.blocks offset: forloop.index -%}
										{%- if block.type != 'text_item' -%}{%- break -%}{%- endif -%}
										{%- if block.type == 'text_item' -%}
											{%- assign t_width 				= block.settings.width -%}
											{%- assign t_heading	 		= block.settings.heading_text -%}
											{%- assign t_heading_fw	 		= block.settings.heading_fw -%}
											{%- assign t_content 			= block.settings.content -%}
											{%- assign t_show_social 		= block.settings.show_social -%}
											{%- capture _t_width -%}{%- include 'set-width-bootstrap',in_type: 'width', number: t_width -%}{{_out}}{%- endcapture -%}

											<div class="textbox-item col-md-{{t_width}}"{{ block.shopify_attributes }}>
												<div class="textbox-item-inner text-{{_t_content_align}}">
													<div class="item-heading fw-{{t_heading_fw}}">{{t_heading}}</div>
													<div class="item-content">
														{%- if t_show_social -%}
															{%- include 'social-media-icons' -%}
														{%- else -%}
															{{t_content}}
														{%- endif -%}
													</div>
												</div>
											</div>
										{%- endif -%}
									{%- endfor -%}
								</div>
							{%- endif -%}
							{%- if _ib_pos == 'text' -%}{{_ib_content}}{%- endif -%}
						</div>

						{%- capture textbox_styles -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .textbox-content-inner', css_value: b_padding -%}
							{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' .textbox-content-inner .heading', css_value: _t_heading_lheight, css_property: 'line-height' -%}
							{{contactTypeID}} .textbox-content-inner{background-color: {{_b_bg_color}};}
							{{contactTypeID}} .textbox-items .item-heading{color:{{_t_heading_color}};}
							{{contactTypeID}} .textbox-items .item-content{color:{{_t_content_color}};}
						{%- endcapture -%}

						{%- assign block_styles = block_styles | append: textbox_styles -%}

			        {%- when 'iconbox' -%}
			        	{%- if has_ib -%}{% continue %}{%- endif -%}

			        	{%- assign ib_bg_image = section.settings.ib_bg_image -%}
			        	{%- capture _ib_bg_image -%}{% if ib_bg_image != blank %}{{ib_bg_image | img_url: '1024x'}}{% endif %}{%- endcapture -%}

						{%- if _ib_pos == 'block' -%}
							<div class="iconbox-content-inner{% if _lazyload and ib_bg_image != blank %} lazyload" data-bgset="{{_ib_bg_image}} 1024w{% endif %}">
								{{_ib_content}}
							</div>
							{%- assign b_padding = section.settings.ib_padding -%}
							{%- unless b_padding == blank -%}
								<style>{%- include 'css-responsive', classID: contactTypeID, nextClassID: ' > div', css_value: b_padding -%}</style>
							{%- endunless -%}
							{%- assign has_ib = true -%}
						{%- endif -%}
						{%- capture ib_styles -%}
							.section-show-{{section.id}} .contact-type-{{block.type}}.block-{{block.id}} .iconbox-content-inner{
								background-color: {{section.settings.ib_bg_color}};
								background-size: {{section.settings.ib_bg_size}};
								background-repeat: {{section.settings.ib_bg_repeat}};
								background-position: {{section.settings.ib_bg_position}};
								{%- unless _lazyload -%}background-image: url({{_ib_bg_image}});{%- endunless -%}
							}
						{%- endcapture -%}

						{%- assign block_styles = block_styles | append: ib_styles -%}

					{%- when 'image' -%}
						{%- assign _image 	= block.settings.image -%}
						{%- assign _image_w = block.settings.image_width -%}

						{%- unless _image == blank -%}
							<div class="image image-{{_image_w}}">
								{%- if _image_w == 'auto' -%}
									{%- include 'image-style', image: _image, set_padding: '115%' -%}
								{%- else -%}
									{%- include 'image-style', image: _image -%}
								{%- endif -%}
							</div>
						{%- endunless -%}

			        {%- else -%}
		        {%- endcase -%}
	        </div>
	      {%- endfor -%}
	    </div>
	</div>
  {%- if _layout == 'boxed' -%}</div>{%- endif -%}

  {%- capture ibID -%}.section-show-{{section.id}} .ib-item{%- endcapture -%}

  	<style>{%- include 'section-general-css' -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-item-contain .ib-icon i' ,css_property: 'font-size' 	,css_value: section.settings.ib_header_max_width -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-header' 	,css_property: 'max-width' 	,css_value: section.settings.ib_header_max_width -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-header'		,css_property: 'margin' 	,css_value: section.settings.ib_heading_margin -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-title' 		,css_property: 'margin' 	,css_value: section.settings.ib_title_margin -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-content' 	,css_property: 'margin' 	,css_value: section.settings.ib_content_margin -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-title' 		,css_property: 'font-size' 	,css_value: section.settings.ib_title_fs -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-content' 	,css_property: 'font-size' 	,css_value: section.settings.ib_content_fs -%}

  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-header'		,css_property: 'color' 	,css_value: section.settings.ib_heading_color -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-title' 		,css_property: 'color' 	,css_value: section.settings.ib_title_color -%}
  	{%- include 'css-responsive', classID: ibID, nextClassID: ' .ib-content' 	,css_property: 'color' 	,css_value: section.settings.ib_content_color -%}
  	{{block_styles}}
	</style>
</div>
{% schema %}
{
	"name":"Contact",
	"tag":"section",
	"settings":[
		{
			"type":"paragraph",
			"content":"Created by 'Contact' section"
		},
		{
			"type":"text",
			"id":"title",
			"label":"Section Label [:id]","default": "Contact"
		},
		{
			"type":"header",
			"content":"LAYOUT"
		},
		{
			"type":"checkbox",
			"id":"boxed_layout",
			"label":"Boxed layout",
			"default":true
		},
	    {
	      "type":"text",
	      "id":"section_padding",
	      "label":"Section Padding",
	      "placeholder":"E.g: 5px 20px 5px 20px",
	      "info":"Order: Top Right Bottom Left"
	    },
		{
			"type":"color",
			"id":"section_bg_color",
			"label":"Background color",
			"default":"#FFFFFF"
		},
		{
			"type":"image_picker",
			"id":"section_bg_image",
			"label":"Background image"
		},
		{
			"type":"select",
			"id":"section_bg_size",
			"label":"Background Size",
			"default":"cover",
			"options":[
				{"value":"auto","label":"Auto"},
				{"value":"cover","label":"Cover"},
				{"value":"contain","label":"Contain"}
			]
		},
		{
			"type":"select",
			"id":"section_bg_repeat",
			"label":"Background Image Repeat:",
			"default":"no-repeat",
			"options":[
				{"value":"no-repeat","label":"No Repeat"},
				{"value":"repeat","label":"Repeat"},
				{"value":"repeat-x","label":"Repeat X"},
				{"value":"repeat-y","label":"Repeat Y"}
			]
		},
		{
			"type":"select",
			"id":"section_bg_position",
			"label":"Background Image Position:",
			"default":"center center",
			"options":[
				{"value":"left top","label":"Left Top"},
				{"value":"center top","label":"Center Top"},
				{"value":"right top","label":"Right Top"},
				{"value":"left center","label":"Left Center"},
				{"value":"center center","label":"Center Center"},
				{"value":"right center","label":"Right Center"},
				{"value":"left bottom","label":"Left Bottom"},
				{"value":"center bottom","label":"Center Bottom"},
				{"value":"right bottom","label":"Right Bottom"}
			]
		},
		{
			"type":"header",
			"content":"Section Heading"
		},
		{
			"type":"text",
			"id":"heading_text",
			"label":"Heading"
		},
